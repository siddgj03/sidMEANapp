  <attribute name="SQL" type="STRING">SELECT 
    trim(DIVACCOUNT_V02.COU_ID) Customer_Country_Code, 
    trim(DIVACCOUNT_V02.DIV_ID) Customer_Division_Code, 
    trim(DIVACCOUNT_V02.ACC_ID) Customer_Account_Number, 
    trim(DIVACCOUNT_V02.COM_ID) Company_Code, 
    trim(DIVACCOUNT_V02.ACG_ID) Account_Group_Code, 
    trim(DIVACCOUNT_V02.COU_ID_RPARENT) Rate_Parent_Country_Code, 
    trim(DIVACCOUNT_V02.ACC_ID_RPARENT) Rate_Parent_Account_Number, 
    trim(DIVACCOUNT_V02.ACC_MRKT_PARTN_CD) Rate_Parent_Account_Grouping_Code, 
    trim(MARKETPARTITION_V01.TAV_DS) Rate_Parent_Account_Grouping_Desc, 
    trim(COUNTRYGROUPSET_V01.COU_NM) Customer_Country_Name,
	case  when trim(DIVACCOUNT_V02.COU_ID) = trim(DIVACCOUNT_V02.COU_ID_RPARENT) and 
 	trim(DIVACCOUNT_V02.ACC_ID) &lt;&gt; trim(DIVACCOUNT_V02.ACC_ID_RPARENT) then &apos;C&apos; else &apos;P&apos; end as &quot;Parent_Child_Indicator&quot;,
	trim(DIVISION_V01.DIV_NM)  Customer_Division_Name
FROM      
    /shared/Source/EDW/RatingDatamart/PRODRATING_P/DIVACCOUNT_V02 DIVACCOUNT_V02 LEFT JOIN 
        /shared/Source/EDW/RatingDatamart/PRODRATING_P/MARKETPARTITION_V01 MARKETPARTITION_V01
    ON DIVACCOUNT_V02.ACC_MRKT_PARTN_CD = MARKETPARTITION_V01.TAV_VALUE_ID LEFT JOIN 
        /shared/Source/EDW/RatingDatamart/PRODRATING_P/COUNTRYGROUPSET_V01 COUNTRYGROUPSET_V01
    ON DIVACCOUNT_V02.COM_ID = COUNTRYGROUPSET_V01.COM_ID AND 
        DIVACCOUNT_V02.COU_ID = COUNTRYGROUPSET_V01.COU_ID    LEFT JOIN
	/shared/Source/EDW/RatingDatamart/PRODRATING_P/DIVISION_V01 DIVISION_V01 ON
		DIVACCOUNT_V02.COM_ID=DIVISION_V01.COM_ID AND DIVACCOUNT_V02.DIV_ID=DIVISION_V01.DIV_ID</attribute>
