  <attribute name="SQL" type="STRING">SELECT 
    trim(RATECARDVERSION_V02.COM_ID) Company_Code, 
    trim(RATECARDVERSION_V02.DIV_ID) Division_Code, 
    trim(RATECARDVERSION_V02.RGP_ID) Ratecard, 
    RATECARDVERSION_V02.RCG_START_DT Ratecard_Version_Start_Date, 
    RATECARDVERSION_V02.RCG_END_DT Ratecard_Version_End_Date, 
    trim(RATECARDVERSION_V02.RCG_SIMPLE_TARF_CD) Tariff_Code, 
    trim(RATECARDVERSION_V02.RGP_VERSION_ID) Ratecard_Version, 
    trim(SIMPLIFIEDTARIFF_V01.TAV_DS) Tariff_Desc,
	trim(DIVISION_V01.DIV_NM) Division_Name,
	trim(RATECARDVERSION_V02.RCG_SEQ_ID) RCG_SEQ_ID
FROM      
    
	/shared/Source/EDW/RatingDatamart/PRODRATING_P/DIVISION_V01 DIVISION_V01 RIGHT OUTER JOIN
	/shared/Source/EDW/RatingDatamart/PRODRATING_P/RATECARDVERSION_V02 RATECARDVERSION_V02
	ON (RATECARDVERSION_V02.COM_ID = DIVISION_V01.COM_ID and 
		RATECARDVERSION_V02.DIV_ID = DIVISION_V01.DIV_ID)LEFT OUTER JOIN 
	/shared/Source/EDW/RatingDatamart/PRODRATING_P/SIMPLIFIEDTARIFF_V01 SIMPLIFIEDTARIFF_V01
    ON RATECARDVERSION_V02.COM_ID = SIMPLIFIEDTARIFF_V01.COM_ID AND 
        RATECARDVERSION_V02.RCG_SIMPLE_TARF_CD = SIMPLIFIEDTARIFF_V01.TAV_VALUE_ID AND
	    &apos;EN&apos; = SIMPLIFIEDTARIFF_V01.LNG_ID and &apos;Y&apos; &lt;&gt; SIMPLIFIEDTARIFF_V01.TAV_NON_USE_IN
</attribute>
