	CAST(REASON_SHORT_DS AS CHAR(5))       AS Reason_Short_Ds,
	CAST(REASON_LONG_DS AS CHAR(60))        AS Reason_Long_Ds
	
 FROM
 
 (SELECT
  
  &apos;N&apos;                        AS MP_INDICATOR,
  &apos;E&apos;                        AS MP_INDICATOR_SHORT_DS,
  CAST(&apos;Extra&apos; AS CHAR(9))   AS MP_INDICATOR_LONG_DS,
  RN.RSN_REASON_CD           AS REASON_ID,
  RN.RSN_REASON_CD           AS REASON_SHORT_DS,
  RN.RSN_RL_DS               AS REASON_LONG_DS

  FROM  /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01  RN
  WHERE RN.LNG_ID = &apos;EN&apos;
  AND RN.RSN_REASON_CD LIKE &apos;NT%&apos;
  
  UNION
  
  SELECT

  &apos;Y&apos;            AS MP_INDICATOR,
  &apos;S&apos;            AS MP_INDICATOR_SHORT_DS,
  &apos;Scheduled&apos;    AS MP_INDICATOR_LONG_DS,
  &apos; &apos;            AS REASON_ID,
  &apos; &apos;            AS REASON_SHORT_DS,
  &apos; &apos;            AS REASON_LONG_DS

  FROM /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01  RN
