       --CM_FTP_COST_MEASURES_V01.DIV_DIVISION_CD  as  Division_Code,
       --CM_FTP_COST_MEASURES_V01.PRD_PRODUCT_CD  as  Product_Code,
       CM_FTP_COST_MEASURES_V01.OUP_COST_SPLIT_CD  as  &quot;Cost_Split_Code&quot;,
       CM_FTP_COST_MEASURES_V01.CTM_SRKY_ID  as  &quot;Cost_Item_Code&quot;,
       T4.CTM_NM  as  &quot;Cost_Item_Description&quot;,
       CM_FTP_COST_MEASURES_V01.CTM_COST_GROUP_CD  as  &quot;Cost_Group_Code&quot;,
       case when CM_FTP_COST_MEASURES_V01.CTM_COST_GROUP_CD = &apos;VA&apos; then &apos;HEADLINE ADJUSTMENT&apos;
         else T5.TAV_DS
       end  as  &quot;Cost_Group_Description&quot;,
       --CM_FTP_COST_MEASURES_V01.CTM_FAP_IN  as  FAP_indicator,
       CM_FTP_COST_MEASURES_V01.CTM_UO_IN  as  &quot;UO_Indicator&quot;,
       CM_FTP_COST_MEASURES_V01.CEK_AM  as  &quot;Cost_Amount&quot;,
       T6.CTM_COST_TYP_CD  as  Cost_Type,
       T7.CTM_SRKY_ID_RELTD  as  &quot;Related_SRKY_ID&quot;,
       --case when ((T7.CTM_COST_TYP_CD &lt;&gt; &apos;UO&apos;) and (not (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD in (&apos;SUM&apos;,&apos;MAN&apos;)))) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0.00&apos; end   as  CM_CHARGE,
