  <attribute name="SQL" type="STRING">SELECT * FROM(SELECT
	CAST(DLY_ID_ORIG AS CHAR(5))               AS Dly_Id_Orig,
	CAST(DLY_ORIG_SHORT_DS AS CHAR(64))          AS Dly_Orig_Short_Ds,
	CAST(DLY_ORIG_LONG_DS AS CHAR(64))            AS Dly_Orig_Long_Ds,
	CAST(DLY_ORIG_CONTR_IN AS CHAR(1))               AS Dly_Orig_Contr_In,
	CAST(DLY_ORIG_CONTR_DS  AS CHAR(15))              AS Dly_Orig_Contr_Ds 
	,ROW_NUMBER() over(partition by DLY_ID_ORIG,DLY_ORIG_CONTR_IN,DLY_ORIG_CONTR_DS order by DLY_ORIG_SHORT_DS,DLY_ORIG_LONG_DS )    as row_nr

  FROM
  (SELECT
     RN.RSN_REASON_CD           AS DLY_ID_ORIG,
     RN.RSN_RL_DS           AS DLY_ORIG_SHORT_DS,
     CASE WHEN RN.RSN_TYPE_CD = &apos;DPDLY&apos; THEN 
         &apos;dep.&apos; ||TRIM(RN.RSN_RL_DS)  
     ELSE
        &apos;arr.&apos; ||TRIM(RN.RSN_RL_DS)  
     END AS DLY_ORIG_LONG_DS,

        CASE WHEN TRIM(RN.RSN_REASON_CD) IN (&apos;DDR&apos; , &apos;DDT&apos; , &apos;DLA&apos; , &apos;DPP&apos; , &apos;DTB&apos; , &apos;DVT&apos; ) THEN
	&apos;Y&apos;
	ELSE 
	&apos;N&apos; 
	END AS DLY_ORIG_CONTR_IN,

      CASE WHEN TRIM(RN.RSN_REASON_CD) IN (&apos;DDR&apos; , &apos;DDT&apos; , &apos;DLA&apos; , &apos;DPP&apos; , &apos;DTB&apos; , &apos;DVT&apos; ) THEN
	&apos;Controlable&apos;
	ELSE 
	&apos;Non-controlable&apos; 
	END AS DLY_ORIG_CONTR_DS


    FROM /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01   RN
    WHERE RN.LNG_ID = &apos;EN&apos;
  --  AND RN.RSN_TYPE_CD = &apos;DPDLY&apos;
  ) D_O
)AS A WHERE ROW_NR=1</attribute>
