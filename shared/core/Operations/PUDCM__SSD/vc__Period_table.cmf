  <attribute name="SQL" type="STRING">SELECT
 DTE_DT AS Dte_Dt
,TRIM(CASE WHEN DTE_DAY_NR = 7 THEN
 0 
ELSE DTE_DAY_NR END) AS Dte_Day_Nr

,TRIM(DTE_DAY_NM) AS Dte_Day_Nm

,TRIM(DTE_YEAR_NR_PLR) AS Dte_F_Year

,TRIM(DTE_QUARTER_NR_PLR) AS Dte_F_Quarter

,TRIM(DTE_YEAR_NR_PLR) || &apos; - Q&apos; ||  TRIM(DTE_QUARTER_NR_PLR) AS Dte_F_Quarter_Ds

,TRIM(DTE_MONTH_NR_PLR) AS Dte_F_Month

,TRIM(DTE_YEAR_NR_PLR) || &apos; - &apos; ||  SUBSTR(DTE_MONTH_NM_PLR,1,3) AS Dte_F_Month_Ds

,TRIM(DTE_WEEK_NR_PLR) AS Dte_F_Week

,TRIM(DTE_YEAR_NR_PLR) || &apos; - &apos; ||  TRIM(DTE_WEEK_NR_PLR) AS Dte_F_Week_Ds 
,cast(format_date(DTE_DT,&apos;YYYY-MM-DD&apos;) as char(10)) AS Rod_Active_Dt

 FROM /shared/Source/EDW/PUDCM/PRODPUDCM_P/CALENDAR_V01  AS DIM_PERIOD
--WHERE DTE_DT &gt;= &apos;2008-01-01&apos; AND EXTRACT( YEAR FROM DTE_DT) &lt;= EXTRACT (YEAR FROM CURRENT_DATE) + 1
WHERE DTE_DT &gt;= CAST( EXTRACT (YEAR FROM CURRENT_DATE) -2 || &apos;-01-01&apos; AS DATE) AND EXTRACT( YEAR FROM DTE_DT) &lt;= EXTRACT (YEAR FROM CURRENT_DATE) + 1
</attribute>
