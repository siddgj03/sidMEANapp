  <attribute name="SQL" type="STRING">SELECT
          C1  As &quot;Con_Product_Group&quot;
         ,C2   AS &quot;Con_Div_Id&quot;
         ,C3   As &quot;Con_Div_Name&quot;
         ,C4  AS &quot;Con_Product_Id&quot;
         ,C5  AS &quot;Con_Product_Name&quot;
         ,C6  AS  &quot;Con_DivProd_Id&quot;
         ,C7  AS &quot;Con_Seq_Nr&quot;

 FROM
  (SELECT
        (CASE when   T2.PRG_ID = &apos;YD&apos; then &apos;International - OPP&apos;
        when   T2.PRG_ID = &apos;JU&apos; then &apos;Domestic&apos;
                                else &apos;Others&apos; end)       as C1
      , T1.div_id                   as C2
      , T3.div_nm                   as C3
      , T1.prd_id                   as C4
      , T4.lpr_nm                   as C5
      , T1.div_id || T1.prd_id      as C6
      ,(CASE when   T2.PRG_ID = &apos;JU&apos; then &apos;1&apos;
        when   T2.PRG_ID = &apos;YD&apos; then &apos;2&apos;
                                else &apos;3&apos;
             end)       as C7
   FROM  /shared/Source/EDW/Premium/PRODPREMIUM_P/PRODUCT_V01  T1
          left outer join
  (Select prg_id, div_id, prd_id
  From /shared/Source/EDW/Premium/PRODPREMIUM_P/PRDPRGPRD_V01
  Where prg_id in (&apos;YD&apos;, &apos;JU&apos;)) T2
   on T1.prd_id = T2.prd_id
   and  T1.div_id = T2.div_id
Left Outer Join  /shared/Source/EDW/Premium/PRODPREMIUM_P/DIVISION_V01  T3
   on T1.Div_id = T3.Div_id
Left Outer Join /shared/Source/EDW/Premium/PRODPREMIUM_P/PRODNAME_V01 T4
   on T1.Div_id = T4.Div_id
  and T1.Prd_Id = T4.Prd_id) AS DIVMAIN
Group by 7,1,2,3,4,5,6

UNION

SELECT
          C1  As &quot;Con_Product_Group&quot;
         ,C2   AS &quot;Con_Div_Id&quot;
         ,C3   As &quot;Con_Div_Name&quot;
         ,C4  AS &quot;Con_Product_Id&quot;
         ,C5  AS &quot;Con_Product_Name&quot;
         ,C6  AS  &quot;Con_DivProd_Id&quot;
         ,C7  AS &quot;Con_Seq_Nr&quot;
 FROM
  (SELECT
        &apos;Incomplete&apos;      as C1
      , T1.div_id               as C2
      , T1.div_id       as C3
      , &apos; &apos;                   as C4
      , &apos; &apos;         as C5
      , T1.div_id || &apos; &apos;      as C6
      , &apos;4&apos;                   as C7
   FROM   /shared/Source/EDW/Premium/PRODPREMIUM_P/DIVISION_V01 T1) AS DIVONLY
Group by 7,1,2,3,4,5,6

UNION

SELECT
          C1  As &quot;Con_Product_Group&quot;
         ,C2   AS &quot;Con_Div_Id&quot;
         ,C3   As &quot;Con_Div_Name&quot;
         ,C4  AS &quot;Con_Product_Id&quot;
         ,C5  AS &quot;Con_Product_Name&quot;
         ,C6  AS  &quot;Con_DivProd_Id&quot;
         ,C7  AS &quot;Con_Seq_Nr&quot;
 FROM
  (SELECT
        &apos;Incomplete&apos;          as C1                                 
      , &apos;NA&apos;                  as C2
      , &apos;No Division&apos;         as C3
      , &apos; &apos;                   as C4
      , &apos; &apos;                   as C5
      , &apos;NA &apos;                 as C6
      , &apos;5&apos;                   as C7) AS NODIV
Group by 7,1,2,3,4,5,6</attribute>
