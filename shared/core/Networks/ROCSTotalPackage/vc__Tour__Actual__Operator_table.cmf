  <attribute name="SQL" type="STRING">SELECT 
  T1.BUP_ID Business_Partner_ID,
T1.BUP_NM Business_Partner_Name,
case 
         when upper(trim(T1.BUP_OWN_COMPANY_IN)) = &apos;Y&apos; then &apos;Yes&apos;
         when upper(trim(T1.BUP_OWN_COMPANY_IN)) = &apos;N&apos; then &apos;No&apos;
         else &apos;Unknown&apos;
       end  as   Business_Partner_Own_Company_Indicator,
T1.BUP_OBJ_ID  Business_Partner_Object_ID

FROM /shared/Source/EDW/ROCSPackage/PRODROCS_P/BUSINESS_PARTNER_V01  T1
inner join
/shared/Core/Networks/ROCSTotalPackage/vc_TourTP T2 ON
 --    T2.Operator_Object_Scheduled_ID=T1.BUP_OBJ_ID
     T2.Operator_Object_Actual_Id=T1.BUP_OBJ_ID
where not (T1.BUP_OBJ_ID is NULL)

