  <attribute name="SQL" type="STRING">SELECT 
T1.CUS_ID as Customer_Number,
T1.CUS_DERIVED_CUSTOMER_NM as Customer_Name,
T2.ACY_ID as Unique_Activity_Number,
T2.ACY_PRIORITY_NR as Activity_Priority_Number,
T2.ATP_ID as Activity_Type,
T3.ATP_DS as Activity_Type_Description,
T2.SLC_ID as Activity_Sales_Category_Code,
T4.SLC_DS as Activity_Sales_Category_Name,
T2.ACY_PLAN_REACT_CD AS Planned_Indicator,
(case  when   T11.&quot;DTE_YRWK_NR_KPN&quot; is NOT null 
		then T11.&quot;DTE_YRWK_NR_KPN&quot; 
	else T12.&quot;DTE_YRWK_NR_KPN&quot; end ) as Activity_Reporting_Week,
T12.DTE_YRWK_NR_KPN  as Activity_Planned_Week,
T11.DTE_YRWK_NR_KPN as Activity_Completed_Week,
T2.ACY_PLANNED_ST_DT as Activity_Planned_Date,
T2.ACY_COMPLETED_DT as Activity_Completed_Date,
T2.ACY_COMPLETED_TD as Activity_Completed_Time,
T2.EMP_ID_ALLOC as Employee_Id,
T13.EMP_FIRST_NM as Employee_First_Name,
T13.EMP_SUR_NM as Employee_Last_Name,
T14.EMP_ID as Activity_Owner,
T15.EMP_FIRST_NM as Activity_Owner_First_Name,
T15.EMP_SUR_NM as Activity_Owner_Last_Name,
T2.ACY_UPDT_USER_ID as Activity_Update_User,
T2.ACY_DELETE_IN as Activity_Delete_Indicator                           
FROM /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_V02 T1 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_ACTIVITY_SLS_V01 T2 
on T1.&quot;CUS_ID&quot; = T2.&quot;CUS_ID&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CALENDAR_V01 T11 
on T2.&quot;ACY_COMPLETED_DT&quot; = T11.&quot;DTE_DT&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CALENDAR_V01 T12 
on T2.&quot;ACY_PLANNED_ST_DT&quot; = T12.&quot;DTE_DT&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/ACTIVITY_TYPE_V01 T3 
on T2.&quot;ATP_ID&quot; = T3.&quot;ATP_ID&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/SALESCATEGORY_V01 T4 
on T2.&quot;SLC_ID&quot; = T4.&quot;SLC_ID&quot; and &apos;Y&apos; &lt;&gt; T4.&quot;SLC_DELETE_IN&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CS_EMPLOYEE_V01 T13 
on T2.&quot;COU_ISO_ID_ALLOC&quot; = T13.&quot;COU_ISO_ID&quot; and T2.&quot;EMP_ID_ALLOC&quot; = T13.&quot;EMP_ID&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_ACTIVITY_EMP_V01 T14 
on T2.&quot;ACY_ID&quot; = T14.&quot;ACY_ID&quot; 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CS_EMPLOYEE_V01 T15 
on T14.&quot;COU_ISO_ID&quot; = T15.&quot;COU_ISO_ID&quot; and T14.&quot;EMP_ID&quot; = T15.&quot;EMP_ID&quot;</attribute>
