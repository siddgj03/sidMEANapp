       D3.Reviewer_1  as  &quot;Reviewer_1&quot;,
       D3.Reviewer_2  as  &quot;Reviewer_2&quot;,
       D2.TNT_Months_Difference  as  &quot;TNT_Months_Difference&quot;,
       coalesce(D2.Reason,D3.Reason)  as  Reason,
       D3.Decision_ID  as  &quot;Decision_ID&quot;
 from 
        	/shared/Core/Finance/FTP/vs_SAS70MonthlyFTPAdjustmentProcessD303		 D3
        full outer join 			/shared/Core/Finance/FTP/vs_SAS70MonthlyFTPAdjustmentProcessD2 D2
        on (((D3.sc = D2.sc) and (D3.sc2 = D2.sc2)) and (D3.sc17 = D2.sc14))*/









 select 
T3.MRQ_ID,
T1.TNT_YR &quot;Year&quot; , T1.TNT_MTH &quot;Month&quot; , 
REP12C_CM_SAS70_RPT12_V01.ADJ_USER_ID Posted_By , 
REP12C_CM_SAS70_RPT12_V01.ADJ_FILE_ID Request_ID , 
T3.MRQ_DT Posted_Date , 
(case  when REP12C_CM_SAS70_RPT12_V01.ADJ_STATUS_CD = &apos;N&apos; then &apos;New Applied&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_STATUS_CD = &apos;O&apos; then &apos;Old Applied&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_STATUS_CD = &apos;P&apos; then &apos;New Pending&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_STATUS_CD = &apos;R&apos; then &apos;Rejected&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_STATUS_CD = &apos;C&apos; then &apos;Cancelled&apos; else NULL end ) Status , 
(case  when REP12C_CM_SAS70_RPT12_V01.ADJ_RECORD_TYPE_CD = &apos;M&apos; then &apos;Manual Adjustment&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_RECORD_TYPE_CD = &apos;S&apos; then &apos;Summary Adjustment&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_RECORD_TYPE_CD = &apos;U&apos; then &apos;U/O&apos; when REP12C_CM_SAS70_RPT12_V01.ADJ_RECORD_TYPE_CD = &apos;C&apos; then &apos;CSA&apos; else NULL end ) &quot;Type&quot; , 
 T1.MTHS_DIFF TNT_Months_Difference , 
 T3.MRQ_REASON_TX Reason  , 
 REP12C_CM_SAS70_RPT12_V01.ADJ_TOTAL_RECORDS_CT Record_Count , 
 REP12C_CM_SAS70_RPT12_V01.ADJ_TOTAL_AM FTP_Amount 
 from (((/shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_SAS70_RPT12_V01 REP12C_CM_SAS70_RPT12_V01 
INNER JOIN /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_OLM_REQUESTS_V01 T3 
on REP12C_CM_SAS70_RPT12_V01.ADJ_FILE_ID = T3.MRQ_ID) 
INNER JOIN /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_COSTINGRUN_V01 T6 
on REP12C_CM_SAS70_RPT12_V01.MRN_ID = T6.MRN_ID) 
INNER JOIN (
select distinct cast(TNTCALDAYSDIFF_V01.TNT_YR || TNTCALDAYSDIFF_V01.TNT_WK as integer) TNT_YRWK_NR ,
 TNTCALDAYSDIFF_V01.TNT_MTH TNT_MTH , 
 TNTCALDAYSDIFF_V01.TNT_YR TNT_YR , 
 TNTCALDAYSDIFF_V01.MTHS_DIFF MTHS_DIFF 
 from /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/TNTCALDAYSDIFF_V01 TNTCALDAYSDIFF_V01) T1 
on T1.TNT_YRWK_NR = T6.MRN_START_WK_NR)</attribute>
