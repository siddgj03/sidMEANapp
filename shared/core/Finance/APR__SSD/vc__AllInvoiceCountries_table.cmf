  <attribute name="SQL" type="STRING">select 
  Region_Code,
  Region_Name,
  Operating_Unit_Code,
  Operating_Unit_Name ,
  Reporting_Unit_Code ,
  Reporting_Unit_Name,  
  Invoice_Country_Code,
  Invoice_Country_Description,
  CONCAT(&apos;APR-Reconciliation-&apos;,case when  Region_Code = &apos;&apos;  then  &apos;Unknown Region&apos;  else Region_Code end)  as Cube_Group_Region_Name,
  CONCAT(&apos;APR-Reconciliation-&apos;,case when  Operating_Unit_Code = &apos;&apos;  then &apos;Unknown Operating Unit&apos;  else Operating_Unit_Code end)  as Cube_Group_Operating_Unit_Name,
  case when Reporting_Unit_Code = &apos;&apos;  then &apos;&apos; else CONCAT(&apos;APR-Reconciliation-&apos;,Reporting_Unit_Code)  end as Cube_Group_Reporting_Unit_Name,
  CONCAT(&apos;APR-Reconciliation-&apos;,case when  Invoice_Country_Code = &apos;&apos;  then  &apos;Unknown Country&apos;  else  Invoice_Country_Code end)  as Cube_Group_Country_Name
  from
  (
SELECT
  T1.DVH_REG_ID AS Region_Code,  --Region Code
  T1.DVH_REG_NM AS Region_Name,  --Region Name  
  T1.DVH_OPU_ID AS Operating_Unit_Code,  --Operating Unit Code
  T1.DVH_OPU_NM AS Operating_Unit_Name,  --Operating Unit Name
  CASE WHEN T1.DVH_TTT_NM LIKE (&apos;*%&apos;)
       THEN NULL
       ELSE T1.DVH_TTT_ID
  END AS Reporting_Unit_Code,   --Reporting Unit Code
  CASE WHEN T1.DVH_TTT_NM LIKE (&apos;*%&apos;)
       THEN NULL
       ELSE T1.DVH_TTT_NM
  END  AS Reporting_Unit_Name,   --Reporting Unit Name  
  T1.COU_ID AS Invoice_Country_Code,  --Country Code
  T1.COU_NM AS Invoice_Country_Description  --Country Code Description

  FROM 
	/shared/Source/EDW/APR/PRODAPR_P/COUNTRYGROUPSET_V01 T1
  
  WHERE T1.COM_ID = &apos;WW&apos;
  GROUP BY 1,2,3,4,5,6,7,8) a     </attribute>
