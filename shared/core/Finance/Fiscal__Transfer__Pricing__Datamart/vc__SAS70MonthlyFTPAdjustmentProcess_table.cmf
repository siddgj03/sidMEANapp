  <attribute name="SQL" type="STRING">/*select 
       D2.Year3  as  &quot;Year&quot;,
       D2.Month4  as  &quot;Month&quot;,
       coalesce(D2.Posted_By,D3.Posted_By)  as  &quot;Posted_By&quot;,
