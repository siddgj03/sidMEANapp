  <attribute name="SQL" type="STRING">SELECT
     CONCAT(RN.RSN_REASON_CD, CONCAT(&apos;_&apos; , (DBP.AVO_DS))) AS CST_TYPE_CD
     ,(RN.RSN_RL_DS)                  		AS CST_SHORT_DS
     ,(RN.RSN_RL_DS)				AS CST_LONG_DS
    
    FROM /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01 RN
    
    CROSS JOIN
    
    ( 
      SELECT DISTINCT BCD.AVO_DS
      FROM            /shared/Source/EDW/ROCS/PRODROCS_P/IMPLEMENTATION_V01 IMP
      INNER JOIN      /shared/Source/EDW/ROCS/PRODROCS_P/BP_CODEDESCRIPTION_V01 BCD
      ON              BCD.AVO_OBJ_ID = IMP.BPM_ENTITY_OBJ_ID
      AND             BCD.AVO_SUBTYPE_CD = &apos;IBE&apos;
    ) DBP
    WHERE RN.LNG_ID = &apos;EN&apos;
    AND RN.RSN_TYPE_CD = &apos;CTYPE&apos;

    
    UNION ALL
    
    SELECT
       CONCAT((RKM.TAV_VALUE_ID), CONCAT(&apos;_&apos; , (DBP.AVO_DS)))	AS CST_TYPE_CD
      ,(RKM.TAV_DS)                  			AS CST_SHORT_DS
      ,(RKM.TAV_DS)   					AS CST_LONG_DS
    FROM /shared/Source/EDW/ROCS/PRODROCS_P/RKM_COST_TYPES_V01 RKM
    
    CROSS JOIN
    
    ( 
      SELECT DISTINCT  BCD.AVO_DS 
      FROM             /shared/Source/EDW/ROCS/PRODROCS_P/IMPLEMENTATION_V01 IMP
      INNER JOIN       /shared/Source/EDW/ROCS/PRODROCS_P/BP_CODEDESCRIPTION_V01 BCD
      ON               BCD.AVO_OBJ_ID = IMP.BPM_ENTITY_OBJ_ID
      AND              BCD.AVO_SUBTYPE_CD = &apos;IBE&apos;
    ) DBP
    WHERE RKM.LNG_ID = &apos;EN&apos;
    
    UNION ALL
    

    SELECT
       CONCAT((ADT.IAV_VALUE_ID), CONCAT(&apos;_&apos; , (BCD.AVO_DS)))	AS CST_TYPE_CD	
      ,TRIM(ADT.IAV_DS)                  			AS CST_SHORT_DS	
      ,TRIM(ADT.IAV_DS)   					AS CST_LONG_DS	
    
    FROM       /shared/Source/EDW/ROCS/PRODROCS_P/ADJUSTMENT_TYPES_V01 ADT
    INNER JOIN  /shared/Source/EDW/ROCS/PRODROCS_P/IMPLEMENTATION_V01 IMP
    ON          IMP.BPM_OBJ_ID = ADT.BPM_OBJ_ID
    INNER JOIN  /shared/Source/EDW/ROCS/PRODROCS_P/BP_CODEDESCRIPTION_V01 BCD
    ON          BCD.AVO_OBJ_ID = IMP.BPM_ENTITY_OBJ_ID
    AND         ADT.LNG_ID = &apos;EN&apos;
    AND         BCD.AVO_SUBTYPE_CD = &apos;IBE&apos;</attribute>
