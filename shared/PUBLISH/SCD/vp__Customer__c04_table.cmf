  <attribute name="SQL" type="STRING">SELECT 
T6.COU_ID,
T6.DIV_ID,
T6.COM_ID,
T6.ACG_ID,
T6.NAD_ID_MAIN,
T6.ACC_ID,
T4.DIV_ID as Division_Code,
T4.DIV_NM as Division_Name,
T6.ACC_MRKT_PARTN_CD as Account_Grouping_Code,
CASE WHEN T6.ACC_GAM_IN  =  &apos;Y&apos;  then  &apos;All Global Accounts&apos;  
WHEN (T6.ACC_GAM_IN  &lt;&gt;  &apos;Y&apos; and T6.ACC_IMP_IN = &apos;Y&apos;)  then &apos;All National Major IMP Accounts&apos;  
WHEN (T7.Tav_Ds is NULL or T7.Tav_Ds = &apos;&apos; and T6.ACC_GAM_IN &lt;&gt; &apos;Y&apos;) then &apos;Non Core Segments&apos; 
else  T7.Tav_Ds end AS Customer_Core_Segment_Description,
T6.ACC_INTL_ACC_IN as International_Account_Indicator,
T7.TAV_DS as Account_Grouping_Description,
T10.PFT_ID  as Profile_Type,
T10.RCT_ID_APPLIC  as Rating_Policy 
from /shared/Source/EDW/SCD/PRODSCD_P/DIVISION_V01 T4 LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/DIVACCOUNT_V02 T6
    on T4.DIV_ID = T6.DIV_ID LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/MARKETPARTITION_V01 T7
    ON T7.TAV_VALUE_ID = T6.ACC_MRKT_PARTN_CD and T7.LNG_ID = &apos;EN&apos; LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/DIVACCRATINGPROFILE_V01 T10
    on T6.COU_ID = T10.COU_ID and T6.ACC_ID = T10.ACC_ID 
	and T6.DIV_ID = T10.DIV_ID and T6.COM_ID = T10.COM_ID and T6.ACG_ID = T10.ACG_ID</attribute>
