  <attribute name="SQL" type="STRING">
    --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD DestinationBUCode, 
    --&apos; d.&apos; || OPPDEPOTREGION_V01.BUSINESS_UNIT_CD DestinationBULabel, 
    --OPPDEPOTREGION_V01.REGION_CD DestinationBURegionCode, 
   --&apos; d.&apos; || OPPDEPOTREGION_V01.REGION_DS DestinationBURegionLabel, 
   --&apos; d.&apos; || OPPDEPOTREGION_V01.REGION_CD DestinationBURegionShortName, 
    --OPPDEPOTREGION_V01.COUNTRY_GRP_CD DestinationCountryGroupCode, 
   -- &apos; d.&apos; || OPPDEPOTREGION_V01.COUNTRY_GRP_DS DestinationCountryGroupLabel, 
    --&apos; d.&apos; || OPPDEPOTREGION_V01.COUNTRY_GRP_CD DestinationCountryGroupShortName, 	 

select
    OPPDEPOTREGION_V01.COU_ID DestinationCountryCode, 
    &apos; d.&apos; || COUNTRYNAME_V01.LXT_NM DestinationCountryLabel, 
    &apos; d.&apos; || COUNTRYNAME_V01.COU_ID DestinationCountryShortName, 
    OPPDEPOTREGION_V01.AREA_CD DestinationAreaCode, 
    &apos; d.&apos; || OPPDEPOTREGION_V01.AREA_CD DestinationAreaLabel, 
    CASE 
        WHEN OPPDEPOTREGION_V01.BUL_ID = &apos;&apos;
            THEN &apos;Unknown&apos;
        WHEN OPPDEPOTREGION_V01.BUL_ID IS NULL
            THEN &apos;Unknown&apos;
        ELSE OPPDEPOTREGION_V01.BUL_ID
        END DestinationDepotCode, 
    &apos; d.&apos; || CASE WHEN OPPDEPOTREGION_V01.BUL_ID = &apos;&apos; THEN &apos;Unknown&apos; WHEN OPPDEPOTREGION_V01.BUL_ID IS NULL THEN &apos;Unknown&apos; ELSE TRIM(OPPDEPOTREGION_V01.BUL_ID) END DestinationDepotLabel, 
    &apos; d.&apos; || CASE WHEN BUSLOC_V01.BUL_NM = &apos;&apos; THEN &apos;Dest Depot Unknown&apos; WHEN BUSLOC_V01.BUL_NM IS NULL THEN &apos;Dest Depot Unknown&apos; ELSE BUSLOC_V01.BUL_NM END DestinationDepotLongName 
    --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD || OPPDEPOTREGION_V01.REGION_CD || OPPDEPOTREGION_V01.COUNTRY_GRP_CD || OPPDEPOTREGION_V01.COU_ID Unique_Category_For_Destination_Country_Code, 
   --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD || OPPDEPOTREGION_V01.REGION_CD || OPPDEPOTREGION_V01.COUNTRY_GRP_CD || OPPDEPOTREGION_V01.COU_ID || OPPDEPOTREGION_V01.AREA_CD Unique_Category_For_Destination_Area_Code, 
    --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD || OPPDEPOTREGION_V01.REGION_CD Unique_Category_For_Destination_BU_Region_Code, 
    --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD || OPPDEPOTREGION_V01.REGION_CD || OPPDEPOTREGION_V01.COUNTRY_GRP_CD Unique_Category_For_Destination_Country_Group_Code, 
    --OPPDEPOTREGION_V01.BUSINESS_UNIT_CD || OPPDEPOTREGION_V01.REGION_CD || OPPDEPOTREGION_V01.COUNTRY_GRP_CD || OPPDEPOTREGION_V01.COU_ID || OPPDEPOTREGION_V01.AREA_CD || CASE WHEN OPPDEPOTREGION_V01.BUL_ID = &apos;&apos; THEN &apos;Unknown&apos; WHEN OPPDEPOTREGION_V01.BUL_ID IS NULL THEN &apos;Unknown&apos; ELSE OPPDEPOTREGION_V01.BUL_ID END Unique_Category_For_Destination_Depot_Code
FROM      
    /shared/Source/EDW/OPS/PRODOPS_P/OPPDEPOTREGION_V01 OPPDEPOTREGION_V01 INNER JOIN 
        /shared/Source/EDW/OPS/PRODOPS_P/BUSLOC_V01 BUSLOC_V01
    ON OPPDEPOTREGION_V01.BUL_ID = BUSLOC_V01.BUL_ID AND 
        OPPDEPOTREGION_V01.COM_ID = BUSLOC_V01.COM_ID INNER JOIN 
        /shared/Source/EDW/OPS/PRODOPS_P/COUNTRYNAME_V01 COUNTRYNAME_V01
    ON ((OPPDEPOTREGION_V01.COU_ID = COUNTRYNAME_V01.COU_ID AND 
        OPPDEPOTREGION_V01.COM_ID = COUNTRYNAME_V01.COM_ID) AND 
        COUNTRYNAME_V01.COU_NON_USE_IN IN (&apos;&apos;, &apos;N&apos;)) AND 
        COUNTRYNAME_V01.LNG_ID = &apos;EN&apos;
WHERE	  
    (OPPDEPOTREGION_V01.BUSINESS_UNIT_CD &lt;&gt; &apos;OTHER&apos; AND 
    OPPDEPOTREGION_V01.COU_ID &lt;&gt; &apos;ZX&apos;) AND 
    OPPDEPOTREGION_V01.COM_ID = &apos;WW&apos;
ORDER BY 
    1 ASC, 
    4 ASC, 
    6 ASC/* 
    7 ASC, 
    9 ASC, 
    11 ASC  */</attribute>
