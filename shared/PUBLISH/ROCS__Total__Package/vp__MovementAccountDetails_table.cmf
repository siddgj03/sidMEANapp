  <attribute name="SQL" type="STRING">select distinct 
           SP.NWK_ID Network_ID, 
           SP.LOC_OBJ_ID_CM Location_Object_ID_CM, 
           MR.MTT_ID Movement_Transion_ID, 
           MR.EBO_MOV_EXTRA_CD EBO_Movement_Extra_Code, 
           MR.EBO_GENLEG_ACCT_CD Account_Number, 
           MR.EBO_COST_CNTR_CD Cost_Centre_Code, 
           MR.EBO_FUNC_AREA_CD Functional_Area_Code, 
           MR.EBO_MATL_GRP_CD Material_Group_Code, 
           ST_DT.Year_Date as SCP_START_DT, 
           case  when END_DT.Year_Date is null then cast ( &apos;2099-12-31&apos; as date )  else END_DT.Year_Date end as SCP_END_DT
     from 
            ( 
                   ( 
                          ( 
/shared/Source/EDW/ROCSPackage/PRODROCS_P/MAPPING_REFERENCE_V01 MR
                        
                          inner  join 
                                 ( select 
                                       SCP_OBJ_ID, 
                                       NWK_ID, 
                                       LOC_OBJ_ID_CM, 
                                       SCP_START_YR, 
                                       SCP_START_WK, 
                                       SCP_END_YR, 
                                       SCP_END_WK
                                 from /shared/Source/EDW/ROCSPackage/PRODROCS_P/SAP_PLANT_V01 
                                      )  SP
                          on MR.SCP_OBJ_ID = SP.SCP_OBJ_ID ) 
                   inner  join 
                            /shared/Core/Networks/ROCSTotalPackage/vc_Calendar ST_DT
                   on SP.SCP_START_YR = ST_DT.Year_Number and SP.SCP_START_WK = ST_DT.Week_Number ) 
            left outer  join 
                   /shared/Core/Networks/ROCSTotalPackage/vc_Calendar END_DT
                     
            on SP.SCP_END_YR = END_DT.Year_Number and SP.SCP_END_WK = END_DT.Week_Number
and  END_DT.Day_Number = 6 ) 
     where MR.EBO_DELETE_IN &lt;&gt; &apos;Y&apos;
    --    on (((((T14.NWK_ID = T10.Network_ID) and (T14.LOC_OBJ_ID_CM = T10.Location_Object_ID_CMBL)) and (T14.MTT_ID = T10.Movement_Transition_ID)) and (T14.EBO_MOV_EXTRA_CD = T10.Mov_Planned_Extra_Indicator)) and (T10.Mov_Financial_Date_Time between T14.SCP_START_DT and T14.SCP_END_DT))
 </attribute>
