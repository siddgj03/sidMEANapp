  <attribute name="SQL" type="STRING">SELECT  ACC_WHITELIST_IN         AS RPP_Whitelist_Exclusion_Flag
              ,&apos;RPP excluded    &apos;       AS RPP_Whitelist_Exclusion_Label
              ,COU_ID || ACC_ID         AS RPP_Whitelist_Account_Code

       FROM    /shared/Source/EDW/OPS/PRODOPS_P/OPERATIONAL_EXCEPTN_V01 OPERATIONAL_EXCEPTN_V01
       WHERE   ACC_WHITELIST_IN = &apos;Y&apos;

      UNION ALL

       SELECT  &apos;N&apos;                      AS RPP_Whitelist_Exclusion_Flag
              ,&apos;Not RPP excluded&apos;       AS RPP_Whitelist_Exclusion_Label
              ,&apos;ZZ&apos;                     AS RPP_Whitelist_Account_Code

       FROM    /shared/Source/EDW/OPS/PRODOPS_P/CALENDAR_V01  CALENDAR_V01
       WHERE   DTE_DT = Current_date</attribute>
