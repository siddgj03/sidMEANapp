  <attribute name="SQL" type="STRING">select distinct 
       T9.ZTR_ID  as  Planned_Tour_ID,
       T9.NWK_ID  as  Planned_Network_ID,
       T9.ZTR_STATUS_CD  as  Planned_Tour_Status_Code,
       T10.Reason_Description  as  Planned_Tour_Status_Desription,


       T9.ZTR_VALID_STA_DT  as  Planned_Tour_Start_Date,
       T9.ZTR_VALID_END_DT  as  Planned_Tour_End_Date,	
T9.ZTD_DW as Planned_Tour_Day_Number      ,
T9.ZTR_OBJ_ID as Planned_Tour_Object_ID

   

 from 
     (select 
           Planned_Tour_2_0__PlannedTour_.ZTR_ID  as  ZTR_ID,
           Planned_Tour_2_0__PlannedTour_.NWK_ID  as  NWK_ID,
           Planned_Tour_2_0__PlannedTour_.ZTR_VALID_STA_DT  as  ZTR_VALID_STA_DT,
           Planned_Tour_2_0__PlannedTour_.ZTR_VALID_END_DT  as  ZTR_VALID_END_DT,
           Planned_Tour_2_0__PlannedTour_.ZTR_STATUS_CD  as  ZTR_STATUS_CD,
           
           T.ZTD_DW   ,
Planned_Tour_2_0__PlannedTour_.ZTR_OBJ_ID
          
         
       
     from /shared/Source/EDW/ROCSPackage/PRODROCS_P/PlannedTour   Planned_Tour_2_0__PlannedTour_,
/shared/Source/EDW/ROCSPackage/PRODROCS_P/PlannedTourOnDay   T

     where 
           (Planned_Tour_2_0__PlannedTour_.ZTR_OBJ_ID = T.ZTR_OBJ_ID)
    )  T9,
      
           /shared/Core/Networks/ROCSTotalPackage/vc_Tour_Reasons T10
--inner join
    
--/shared/Core/Networks/ROCS/vc_Calendar

 where 
       (T9.ZTR_STATUS_CD = T10.Reason_Code) and
      -- (vc_Calendar.Day_Number = T9.ZTD_DW) and
  (T10.Reason_Type_Code in (&apos;ADVST&apos;,&apos;VSNST&apos;))</attribute>
