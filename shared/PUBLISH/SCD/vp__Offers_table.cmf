  <attribute name="SQL" type="STRING">SELECT DISTINCT
	T1.CUS_ID as Customer_Number,
	T1.OFF_ID AS Offer_ID,
	T1.OFF_DS AS Offer_Description,
	 CAST(CAST(T1.&quot;OFF_ANNUAL_REV_AM&quot; AS FLOAT) / T2.&quot;EXR_RATE_AM&quot; AS DECIMAL(18,2)) AS Annual_Value_Euro,
	T1.OFF_OFFER_DT AS Offer_Date,
	T3.OFS_ID AS Status,
	T1.OFF_CANCEL_IN AS Cancelled_Indicator
from /shared/Source/EDW/SCD/PRODSCD_P/SALES_OFFER_V01 T1 LEFT OUTER JOIN
    /shared/Source/EDW/SCD/PRODSCD_P/CC_EXCHANGERATE_V02 T2
ON T1.&quot;CUY_ID&quot; = T2.&quot;CUY_ID&quot; and &apos;MO&apos; = T2.&quot;EGP_ID&quot; 
	and CURRENT_DATE  between T2.&quot;EXR_START_DT&quot; and T2.&quot;EXR_END_DT&quot; LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/SALES_OFFER_HIST_V01 T3
    on T1.&quot;OFF_ID&quot; = T3.&quot;OFF_ID&quot; and &apos;Y&apos; &lt;&gt; T3.&quot;OFH_DELETE_IN&quot;
</attribute>
