  <attribute name="SQL" type="STRING">SELECT
  &apos;U&apos;       Reconciliation_Code,
  &apos;Unmatched&apos;      Reconciliation_Status_Description, 
  CAST(CONCAT(TRIM(PROCESSERROR.FNC_ID),PROCESSERROR.ERR_CD) AS CHAR(10))  as Exclusion_Reason_Code,
  PROCESSERROR.ERR_DS     AS Exclusion_Reason_Description
 
  FROM     /shared/Source/EDW/APR/PRODAPR_P/PROCESSERROR_V01 PROCESSERROR
  WHERE PROCESSERROR.LNG_ID = &apos;EN&apos;
  AND (PROCESSERROR.FNC_ID = &apos;REVREPS&apos; OR PROCESSERROR.FNC_ID = &apos;BDC&apos; OR PROCESSERROR.FNC_ID = &apos;CONCN&apos; OR PROCESSERROR.FNC_ID = &apos;INTERR&apos;)
  GROUP BY 1,2,3,4   

UNION 
  SELECT  
&apos;M&apos; AS Reconciliation_Code,
&apos;Matched&apos; AS Reconciliation_Status_Description,
&apos;&apos; AS Exclusion_Reason_Code,
&apos;&apos; AS Exclusion_Reason_Description
UNION 
	SELECT  
&apos;M&apos; AS Reconciliation_Code,
&apos;Matched&apos; AS Reconciliation_Status_Description,
&apos;CONCN&apos; AS Exclusion_Reason_Code,
&apos;CANCELLED&apos; AS Exclusion_Reason_Description
UNION  
	SELECT  
&apos;U&apos; AS Reconciliation_Code,
&apos;Unmatched&apos; AS Reconciliation_Status_Description,
&apos;CONCN&apos; AS Exclusion_Reason_Code,
&apos;CANCELLED&apos; AS Exclusion_Reason_Description  
UNION
select 
&apos;U&apos; AS Reconciliation_Code,
&apos;Unmatched&apos; AS Reconciliation_Status_Description,
&apos;CONSIGNMENT DATA ERROR&apos; AS Exclusion_Reason_Code,
&apos;CONSIGNMENT DATA ERROR&apos; AS Exclusion_Reason_Description  											 </attribute>
