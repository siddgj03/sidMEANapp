  <attribute name="SQL" type="STRING">select   
		------------------------------
       T1.KYR_ID  as  &quot;Current_Year&quot;,
       T1.KWK_MONTH_NR  as  &quot;Current_Month&quot;,
       T1.CTM_SRKY_ID  as  &quot;Cost_Item_Code&quot;,
       CM_COSTITEM_V01.CTM_NM  as  &quot;Cost_Item_Description&quot;,
       CM_COSTELEMENT_V01.CTM_COST_GROUP_CD  as  &quot;Cost_Group_Code&quot;,
       CM_COSTGROUP_V01.TAV_DS  as  &quot;Cost_Group_Description&quot;,
       T1.UOR_ADJ_FACTOR_AM  as  &quot;Global_Correction_Factor&quot;,
       TNTCALDAYSDIFF_V01.MIN_MTHS_DIFF  as  &quot;Months_Difference&quot;  
 from 	(select 
              CM_RECOVERY_ADJMNT_V01.KYR_ID,
              CM_RECOVERY_ADJMNT_V01.KWK_MONTH_NR,
              CM_RECOVERY_ADJMNT_V01.CTM_SRKY_ID,
              CM_RECOVERY_ADJMNT_V01.UOR_ADJ_FACTOR_AM
        from 
         /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_RECOVERY_ADJMNT_V01   CM_RECOVERY_ADJMNT_V01
        where CM_RECOVERY_ADJMNT_V01.UOT_TYPE_ID = &apos;F&apos; and CM_RECOVERY_ADJMNT_V01.UOR_STATUS_CD in  (&apos;P&apos;, &apos;A&apos;) ) T1
        left outer join   /shared/Core/Finance/Fiscal_Transfer_Pricing_Datamart/vc_Dates  TNTCALDAYSDIFF_V01
        on TNTCALDAYSDIFF_V01.TNT_MTH = T1.KWK_MONTH_NR and TNTCALDAYSDIFF_V01.TNT_YR = T1.KYR_ID 
		
        left outer join 
       (
        /shared/Core/Finance/Fiscal_Transfer_Pricing_Datamart/vc_CostElementY   CM_COSTELEMENT_V01
            join 
        /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_COSTITEM_V01   CM_COSTITEM_V01      
            on (CM_COSTITEM_V01.CTM_SRKY_ID = case when CM_COSTELEMENT_V01.CTM_SRKY_ID_RELTD = 0 then CM_COSTELEMENT_V01.CTM_SRKY_ID
  else CM_COSTELEMENT_V01.CTM_SRKY_ID_RELTD
end)
       )
        on T1.CTM_SRKY_ID = CM_COSTELEMENT_V01.CTM_SRKY_ID and CM_COSTELEMENT_V01.CTM_LATESTVERSN_IN = &apos;Y&apos;
        left outer join 
       /shared/Source/EDW/CostModelling/PRODCOSTMDLG_P/CM_COSTGROUP_V01  CM_COSTGROUP_V01
        on CM_COSTELEMENT_V01.CTM_COST_GROUP_CD = CM_COSTGROUP_V01.TAV_VALUE_ID 
		and CM_COSTGROUP_V01.LNG_ID = &apos;EN&apos; 
		and CM_COSTGROUP_V01.TAV_NON_USE_IN = &apos;N&apos;</attribute>
