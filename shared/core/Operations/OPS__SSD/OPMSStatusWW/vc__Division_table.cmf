  <attribute name="SQL" type="STRING">	SELECT
                CAST(Actual_Product_Group_Code AS CHAR(2)) as Actual_Product_Group_Code
               ,CAST(Actual_Product_Group_Label AS CHAR(17)) as Actual_Product_Group_Label
               ,CAST(Division_Code AS CHAR(3)) as Division_Code
               ,CAST(Actual_Product_Code AS CHAR(4)) as Actual_Product_Code
               ,CAST(Unique_DivProd_Id AS CHAR(7)) as  Unique_DivProd_Id
			,Unique_Category_For_Division_Code
			,Unique_Category_For_Actual_Product_Code
FROM
  (SELECT DISTINCT    (CASE when   T2.PRG_ID = &apos;YD&apos; then &apos;YD&apos;
                            when   T2.PRG_ID = &apos;JU&apos; then &apos;JU&apos;
                            else &apos;ZZ&apos; end)                                    AS Actual_Product_Group_Code
                      ,(CASE  when   T2.PRG_ID = &apos;YD&apos; then &apos;International OPP&apos;
                              when   T2.PRG_ID = &apos;JU&apos; then &apos;Domestic OPP&apos;
                              else &apos;Other&apos; end)                               AS Actual_Product_Group_Label
                      , T1.div_id                                             AS Division_Code
                      , T1.prd_id                                             AS Actual_Product_Code
                      , T1.div_id || T1.prd_id                                AS Unique_DivProd_Id
					,(CASE when   T2.PRG_ID = &apos;YD&apos; then &apos;YD&apos;
                            when   T2.PRG_ID = &apos;JU&apos; then &apos;JU&apos;
                            else &apos;ZZ&apos; end)|| T1.div_id  as Unique_Category_For_Division_Code,
(CASE when   T2.PRG_ID = &apos;YD&apos; then &apos;YD&apos;
                            when   T2.PRG_ID = &apos;JU&apos; then &apos;JU&apos;
                            else &apos;ZZ&apos; end)|| T1.div_id|| T1.prd_id as Unique_Category_For_Actual_Product_Code

   FROM /shared/Source/EDW/OPS/PRODOPS_P/PRODUCT_V01  T1
              left outer join
        /shared/Source/EDW/OPS/PRODOPS_P/PRDPRGPRD_V01 T2
   on  T1.prd_id = T2.prd_id
   and T1.div_id = T2.div_id
   and T2.prg_id in (&apos;YD&apos;, &apos;JU&apos;)
) AS DIVISION
order by Actual_Product_Group_Code,Division_Code, Unique_DivProd_Id, Unique_Category_For_Actual_Product_Code
;</attribute>
