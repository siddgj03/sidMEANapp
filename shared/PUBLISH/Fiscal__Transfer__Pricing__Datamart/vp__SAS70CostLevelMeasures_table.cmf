       --T8.OUP_YR_NR  as  CM_Charge_Year,
       --T7.CTM_ALLOC_TYP_CD  as  Allocation_Type,
       --case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;SUM&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end   as  Summary,
       --case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;MAN&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end   as  Manual,
       --((case when ((T7.CTM_COST_TYP_CD &lt;&gt; &apos;UO&apos;) and (not (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD in (&apos;SUM&apos;,&apos;MAN&apos;)))) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0.00&apos; end  || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;SUM&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end ) || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;MAN&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end )  as  Correction_Factor_Calc,
       --case when (((case when ((T7.CTM_COST_TYP_CD &lt;&gt; &apos;UO&apos;) and (not (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD in (&apos;SUM&apos;,&apos;MAN&apos;)))) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0.00&apos; end  || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;SUM&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end ) || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;MAN&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end ) = 0.00) then &apos;0.00&apos; else (CM_FTP_COST_MEASURES_V01.CEK_AM / ((case when ((T7.CTM_COST_TYP_CD &lt;&gt; &apos;UO&apos;) and (not (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD in (&apos;SUM&apos;,&apos;MAN&apos;)))) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0.00&apos; end  || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;SUM&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end ) || case when (CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;MAN&apos;) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end )) end   as  Correction_Factor,
       --case when ((CM_FTP_COST_MEASURES_V01.OUP_ALLOC_STAT_CD = &apos;SUM&apos;) and (CM_FTP_COST_MEASURES_V01.CTM_UO_IN = &apos;N&apos;)) then CM_FTP_COST_MEASURES_V01.CEK_AM else &apos;0&apos; end   as  Non_UO_Summary,
       T9.TAV_DS  as  &quot;Business_Line_Description&quot;
 from 
       (
           /shared/Core/Finance/Fiscal_Transfer_Pricing_Datamart/vc_CostMeasures CM_FTP_COST_MEASURES_V01
            join 
           (select 
                  T1.OUP_MTH_NR  as  OUP_MTH_NR,
