  <attribute name="SQL" type="STRING">select	TRIM(vc_RPUOverRide.COZ_REP_UNIT_CD) AS COZ_REP_UNIT_CD,
		--vc_CountryNetworkGroupSet.CM_STR_LVL1_ID  as  &quot;Actual_Invoice_Region_Code&quot;,
		--vc_CountryNetworkGroupSet.CM_STR_LVL3_ID  as  &quot;Actual_Invoice_Operating_Unit_Code&quot;,
		--coalesce(vc_RPUOverRide.COZ_REP_UNIT_CD,vc_CountryNetworkGroupSet.CM_STR_LVL5_ID,TRIM(vc_MonthlyMeasures.COU_ID_ACTUAL))  as  &quot;Actual_Invoice_Reporting_Unit_Code&quot;,
		--coalesce(vc_RPUOverRide.COZ_REP_UNIT_CD,vc_CountryNetworkGroupSet.CM_STR_LVL5_NM,vc_CountryNetworkGroupSet.CM_STR_LVL5_ID,TRIM(vc_MonthlyMeasures.COU_ID_ACTUAL))  as  &quot;Actual_Invoice_Reporting_Unit&quot;,
		--TRIM(coalesce(vc_RPUOverRide.COZ_REP_UNIT_CD,vc_CountryNetworkGroupSet.CM_STR_LVL4_ID))  as  &quot;Actual_Invoice_Parent_Country_Code&quot;,
		--TRIM(coalesce(vc_RPUOverRide.COZ_REP_UNIT_CD,vc_CountryNetworkGroupSet.CM_STR_LVL4_NM,vc_CountryNetworkGroupSet.CM_STR_LVL4_ID))  as  &quot;Actual_Invoice_Parent_Country&quot;,
		TRIM(vc_MonthlyMeasures.COU_ID_ACTUAL)  as  &quot;Actual_Invoice_Country_Code&quot;,
		--coalesce(vc_CountryNetworkGroupSet.COU_ENT_NM,TRIM(vc_MonthlyMeasures.COU_ID_ACTUAL))  as  &quot;Actual_Invoice_Country&quot;,
		vc_CountryNetworkGroupSet1.CM_STR_LVL1_ID  as  &quot;Cost_Owner_Region_Code&quot;,
		vc_CountryNetworkGroupSet1.CM_STR_LVL3_ID  as  &quot;Cost_Owner_Operating_Unit_Code&quot;,
		coalesce(vc_CountryNetworkGroupSet1.CM_STR_LVL5_ID,TRIM(vc_MonthlyMeasures.CEK_OWNER_CD))  as  &quot;Cost_Owner_Reporting_Unit_Code&quot;,
		coalesce(vc_CountryNetworkGroupSet1.CM_STR_LVL5_NM,vc_CountryNetworkGroupSet1.CM_STR_LVL5_ID,TRIM(vc_MonthlyMeasures.CEK_OWNER_CD))  as  &quot;Cost_Owner_Reporting_Unit&quot;,
		TRIM(vc_CountryNetworkGroupSet1.CM_STR_LVL4_ID)  as  &quot;Cost_Owner_Parent_Country_Code&quot;,
		TRIM(coalesce(vc_CountryNetworkGroupSet1.CM_STR_LVL4_NM,vc_CountryNetworkGroupSet1.CM_STR_LVL4_ID))  as  &quot;Cost_Owner_Parent_Country&quot;,
		TRIM(vc_MonthlyMeasures.CEK_OWNER_CD)  as  &quot;Cost_Owner_Code&quot;,
		coalesce(vc_CountryNetworkGroupSet1.COU_ENT_NM,TRIM(vc_MonthlyMeasures.CEK_OWNER_CD))  as  &quot;Cost_Owner&quot;,
		vc_MonthlyMeasures.OUP_YR_NR  as  &quot;Current_Year&quot;,
		vc_MonthlyMeasures.OUP_MTH_NR  as  &quot;Current_Month&quot;,
		vc_MonthlyMeasures.OUP_YEAR_MTH_NR  as  &quot;Current_Year_Month&quot;,
		vc_TNTCalDaysDiff.MIN_YRS_DIFF  as  &quot;Years_Difference&quot;,
		vc_TNTCalDaysDiff.MIN_MTHS_DIFF  as  &quot;Months_Difference&quot;,
		vc_MonthlyMeasures.OUP_ALLOC_STAT_CD  as  &quot;Allocation_Status_Code&quot;,
		vc_AllocType.TAV_DS  as  &quot;Allocation_Status&quot;,
		vc_MonthlyMeasures.OUP_BUS_LINE_CD  as  &quot;Business_Line_Code&quot;,
		vc_BusLine.TAV_DS  as  &quot;Business_Line&quot;,
		--vc_MonthlyMeasures.LOB_ID  as  Line_Of_Business_Code,
		--vc_LineOfBussiness.LOB_NM  as  Line_Of_Business,
		--vc_MonthlyMeasures.OUP_EU_IN  as  &quot;EU Indicator Code&quot;,
		case 
			when vc_MonthlyMeasures.OUP_EU_IN = &apos;N&apos; then &apos;Non-EU&apos;
