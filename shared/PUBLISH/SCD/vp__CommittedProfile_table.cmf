  <attribute name="SQL" type="STRING">SELECT DISTINCT 
	T3.CUS_ID as Customer_Number,
	T1.LOB_NM AS Line_of_Business,
	T2.PRG_NM AS Product_Group,
	T3.TRP_REVENUE_AM AS Revenue_Local_Currency,
	CAST((case  when (T3.&quot;TRP_FREQ_CD&quot; = &apos;1&apos;) then (T3.&quot;TRP_REVENUE_AM&quot; * 5) 
				when (T3.&quot;TRP_FREQ_CD&quot; = &apos;2&apos;) then T3.&quot;TRP_REVENUE_AM&quot; 
				when (T3.&quot;TRP_FREQ_CD&quot; = &apos;3&apos;) then (CAST((T3.&quot;TRP_REVENUE_AM&quot; * 12) AS FLOAT) / 52) 
				when (T3.&quot;TRP_FREQ_CD&quot; = &apos;4&apos;) then (CAST((T3.&quot;TRP_REVENUE_AM&quot; * 4) AS FLOAT) / 52) 
				else (CAST(T3.&quot;TRP_REVENUE_AM&quot; AS FLOAT) / 52) end ) AS FLOAT) / T4.&quot;EXR_RATE_AM&quot; AS Weekly_Revenue_Euro,
	T5.TAV_DS AS Frequency,
	T3.TRP_CANCEL_IN AS Cancelled_Indicator,
	T3.TRP_CREATED_TD AS Created_Date
from /shared/Source/EDW/SCD/PRODSCD_P/CC_EXCHANGERATE_V02 T4, 
((/shared/Source/EDW/SCD/PRODSCD_P/CUSTOMERPROFILE_V01 T3 LEFT OUTER JOIN 
/shared/Source/EDW/SCD/PRODSCD_P/LINE_OF_BUSINESS_V01 T1 
on T3.&quot;LOB_ID&quot; = T1.&quot;LOB_ID&quot; 
	and &apos;Y&apos; = T1.&quot;LOB_ACTIVE_IN&quot; 
	and &apos;EN&apos; = T1.&quot;LNG_ID&quot;) LEFT OUTER JOIN 
/shared/Source/EDW/SCD/PRODSCD_P/PRODGROUP_NAME_V01 T2 
on T3.&quot;LOB_ID&quot; = T2.&quot;LOB_ID&quot; 
	and T3.&quot;PRG_ID&quot; = T2.&quot;PRG_ID&quot; 
	and &apos;Y&apos; = T2.&quot;PRG_ACTIVE_IN&quot; 
	and &apos;EN&apos; = T2.&quot;LNG_ID&quot;) LEFT OUTER JOIN 
/shared/Source/EDW/SCD/PRODSCD_P/AV_TRP_FREQ_V01 T5 
on T3.&quot;TRP_FREQ_CD&quot; = T5.&quot;TAV_VALUE_ID&quot; 
	and &apos;EN&apos; = T5.&quot;LNG_ID&quot;
 where T3.&quot;CUY_ID&quot; = T4.&quot;CUY_ID&quot; and T4.&quot;EGP_ID&quot; = &apos;MO&apos; 
	and CURRENT_DATE between T4.&quot;EXR_START_DT&quot; and T4.&quot;EXR_END_DT&quot;






</attribute>
