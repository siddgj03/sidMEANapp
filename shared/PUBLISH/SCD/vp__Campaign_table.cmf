  <attribute name="SQL" type="STRING">SELECT DISTINCT 
	T1.CUS_ID as Customer_Number,
	(case  when  NOT T2.CUS_COMPANY_NM is null or TRIM(TRAILING
 FROM T2.CUS_COMPANY_NM) &lt;&gt; &apos;&apos; then T2.CUS_COMPANY_NM when  NOT T2.CUS_TITLE_CD is null then TRIM(TRAILING
 FROM T2.CUS_TITLE_CD) else &apos;&apos; end ) || (case  when T2.CUS_FORE_NM is null or TRIM(TRAILING
 FROM T2.CUS_FORE_NM) = &apos;&apos; then &apos;&apos; else &apos; &apos; || TRIM(TRAILING
 FROM T2.CUS_FORE_NM) end ) || (case  when T2.CUS_INIT_NM is null or TRIM(TRAILING
 FROM T2.CUS_INIT_NM) = &apos;&apos; then &apos;&apos; else &apos; &apos; || TRIM(TRAILING
 FROM T2.CUS_INIT_NM) end ) || (case  when T2.CUS_SUR_NM is null or T2.CUS_SUR_NM &lt;&gt; &apos; &apos; then &apos;&apos; else &apos; &apos; || TRIM(TRAILING
 FROM T2.CUS_SUR_NM) end )	as Customer_Name,
	T2.COU_ISO_ID as Country_Code,
	T3.COU_NM as Country_Name,
	T4.DTE_YRWK_NR_KPN as Marketing_Communication_Week
from /shared/Source/EDW/SCD/PRODSCD_P/CP_CUST_ACTIVITY_V01 T1 LEFT OUTER JOIN 
       /shared/Core/Sales/SCD/vc_CalendarV01  T4
    on T1.ACTIVITY_DT = T4.DTE_DT,
    /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_V02 T2 LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/CS_COUNTRY_V01 T3
    on T2.COU_ISO_ID = T3.COU_ISO_ID
where T1.CUS_ID = T2.CUS_ID</attribute>
