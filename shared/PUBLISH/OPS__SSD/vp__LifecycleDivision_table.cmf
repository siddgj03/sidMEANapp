  <attribute name="SQL" type="STRING">SELECT DISTINCT 
    CASE 
        WHEN PRDPRGPRD_V01.PRG_ID = &apos;YD&apos;
            THEN &apos;YD&apos;
        WHEN PRDPRGPRD_V01.PRG_ID = &apos;JU&apos;
            THEN &apos;JU&apos;
        ELSE &apos;ZZ&apos;
        END ActualProductGroupCode, 
    CASE 
        WHEN PRDPRGPRD_V01.PRG_ID = &apos;YD&apos;
            THEN &apos;International OPP&apos;
        WHEN PRDPRGPRD_V01.PRG_ID = &apos;JU&apos;
            THEN &apos;Domestic OPP&apos;
        ELSE &apos;Other&apos;
        END ActualProductGroupLabel, 
    PRODUCT_V01.DIV_ID DivisionCode, 
    PRODUCT_V01.PRD_ID ActualProductCode, 
    PRODUCT_V01.DIV_ID || PRODUCT_V01.PRD_ID UniqueDivProdId, 
    CONCAT(CASE WHEN PRDPRGPRD_V01.PRG_ID = &apos;YD&apos; THEN &apos;YD&apos; WHEN PRDPRGPRD_V01.PRG_ID = &apos;JU&apos; THEN &apos;JU&apos; ELSE &apos;ZZ&apos; END, PRODUCT_V01.DIV_ID) UniqueCategoryForDivisionCode
FROM      
    /shared/Source/EDW/OPS/PRODOPS_P/PRODUCT_V01 PRODUCT_V01 LEFT OUTER JOIN 
        /shared/Source/EDW/OPS/PRODOPS_P/PRDPRGPRD_V01 PRDPRGPRD_V01
    ON (PRODUCT_V01.PRD_ID = PRDPRGPRD_V01.PRD_ID AND 
        PRODUCT_V01.DIV_ID = PRDPRGPRD_V01.DIV_ID) AND 
        PRDPRGPRD_V01.PRG_ID IN (&apos;YD&apos;, &apos;JU&apos;)		 
ORDER BY 
    2 ASC, 
    3 ASC, 
    4 ASC, 
    5 ASC</attribute>
