  <attribute name="SQL" type="STRING">SELECT 
T17.&quot;OPP_ID&quot;,
T17.&quot;MPN_DELETE_IN&quot;,
T1.OPP_TITLE_TX as Project_Title,
T1.OPP_ID as Unique_Opportunity_Number,
T1.OPP_RECEIVED_DT as Date_Opportunity_Identified,
CAST(T1.OPP_REVIEW_DT AS DATE) as Review_Date,
T1.OPP_DEADLINE_DT as Deadline,
T5.TAV_DS as Initiator,
T6.TAV_DS as Opportunity_Classification,
T7.TAV_DS as &quot;Type&quot;,
T1.EMP_ID_MAJ_ACC_MGR as MA_Manager_ID,
T9.TAV_DS as Reason_For_Request,
T10.TAV_DS as GAM_Support,
T1.OPP_STNDRD_SOLN_IN as Standard_Solution_Indicator,
T1.OPP_BUS_SOLN_TX as Business_Solution,
T11.LOB_NM as Line_of_Business,
T12.PRG_NM as Product_Group,
T1.OPP_MULTI_COU_IN as Multi_Country_Indicator,
T1.COU_ISO_ID as Lead_Country,
T13.TAV_DS as Decision_Maker_Type,
T1.OPP_DEC_MAKER_NM as Decision_Maker_Name,
T14.TAV_DS as Decision_Maker_Relationship,
T15.CMT_ID as Current_Provider,
(T1.OPP_START_YR * 100 || substring (T1.OPP_ST_WEEK_NR, 1, 2))  as Projected_Start_Year_And_Week,
	T1.OPP_EST_VALUE_AM as Annual_Value_Total,
	T1.CUY_ID as Annual_Value_Currency,
	(CAST(T1.&quot;OPP_EST_VALUE_AM&quot; AS FLOAT) / T16.&quot;EXR_RATE_AM&quot;) as Annual_Value_Total_Euros,
 	(case  when T1.OPP_START_YR = YEAR(current_date) 
		then (CAST(T1.OPP_EST_VALUE_AM AS FLOAT) / 52) * (53 - CAST(T1.OPP_ST_WEEK_NR AS FLOAT)) 
	when T1.OPP_START_YR &gt; YEAR(current_date) then 0 
	when T1.OPP_START_YR &lt; YEAR(current_date) then T1.OPP_EST_VALUE_AM 
		when T1.OPP_STATUS_CD = &apos;CTS&apos; then 0 else 0 end ) as Expected_PL_Total_Local,
	(case  when T1.OPP_START_YR = YEAR(current_date) 
		then ((CAST(T1.OPP_EST_VALUE_AM AS FLOAT)/ T16.&quot;EXR_RATE_AM&quot;) / 52) * (53 - CAST(T1.OPP_ST_WEEK_NR AS FLOAT)) 
	when T1.OPP_START_YR &gt; YEAR(current_date) then 0 
	when T1.OPP_START_YR &lt; YEAR(current_date) then (T1.OPP_EST_VALUE_AM/ T16.&quot;EXR_RATE_AM&quot;) 
		when T1.OPP_STATUS_CD = &apos;CTS&apos; then 0 else 0 end ) as Expected_PL_Total_Euros,
T17.MPN_EST_VALUE_AM AS Line_of_Business_Annual_Value_Total_Local,
CAST(T17.&quot;MPN_EST_VALUE_AM&quot; AS FLOAT) / T16.&quot;EXR_RATE_AM&quot; AS Line_of_Business_Annual_Value_Total_Euros, 
CASE WHEN T1.&quot;OPP_START_YR&quot; = YEAR(CURRENT_DATE) 
