  <attribute name="SQL" type="STRING">SELECT 
	T1.CUS_ID as Customer_Number,
	T1.COU_ISO_ID AS Country_Code,
	T1.SLC_ID AS Sales_Category_Code,
	SLC_DS AS Sales_Category_Name,
	T1.LOB_ID AS Line_of_Business_Code,
	LOB_NM AS Line_of_Business_Name,
	T1.BUL_CSYS_ID AS Sales_Business_Location,
	T1.BUL_ID AS Sales_Business_Location_Code,
	BUL_NM AS Sales_Business_Location_Name,
	T1.STE_ID AS Sales_Territory_Code,
	T5.STE_TYPE_CD AS Sales_Territory_Type,
	T6.STE_TYPE_DS AS Sales_Territory_Type_Desc,
	(case  when T1.STE_DEDGEO_CD = &apos;G&apos; then &apos;Geographic&apos; 
			when T1.STE_DEDGEO_CD = &apos;D&apos; then &apos;Dedicated&apos; else &apos;&apos; end ) AS Customer_Territory_Association    
from ((((/shared/Core/Sales/SCD/vc_CustSalesInfraDly T1 LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/SALESTERRITORY_V01 T5
    on T1.&quot;STE_CSYS_ID&quot; = T5.&quot;STE_CSYS_ID&quot;) LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/SALESCATEGORY_V01 T2
    on T5.&quot;SLC_ID&quot; = T2.&quot;SLC_ID&quot;) LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/LINE_OF_BUSINESS_V01 T3
    on T1.&quot;LOB_ID&quot; = T3.&quot;LOB_ID&quot; and &apos;EN&apos; = T3.&quot;LNG_ID&quot; and &apos;Y&apos; = T3.&quot;LOB_ACTIVE_IN&quot;) LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/BUSINESSLOCATION_V01 T4
    on T1.&quot;BUL_CSYS_ID&quot; = T4.&quot;BUL_CSYS_ID&quot;) LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/SALESTERR_TYPE_V01 T6
    on T5.&quot;STE_TYPE_CD&quot; = T6.&quot;STE_TYPE_CD&quot;</attribute>
