  <attribute name="SQL" type="STRING">SELECT 
    OPPDEPOTREGION_V01.business_unit_cd as OriginBUCode,
      &apos; o.&apos; || OPPDEPOTREGION_V01.business_unit_cd as OriginBULabel,
      OPPDEPOTREGION_V01.region_cd as OriginBURegionCode,
      &apos; o.&apos; || OPPDEPOTREGION_V01.region_ds as OriginBURegionLabel,
      &apos; o.&apos; || OPPDEPOTREGION_V01.region_cd as OriginBURegionShortName,
      OPPDEPOTREGION_V01.country_grp_cd as OriginCountryGroupCode,
      &apos; o.&apos; || OPPDEPOTREGION_V01.country_grp_ds as OriginCountryGroupLabel,
      &apos; o.&apos; || OPPDEPOTREGION_V01.country_grp_cd as OriginCountryGroupShortName,
      OPPDEPOTREGION_V01.cou_id as OriginCountryCode,
      &apos; o.&apos; || COUNTRYNAME_V01.lxt_nm  as OriginCountryLabel,
      &apos; o.&apos; || COUNTRYNAME_V01.cou_id as OriginCountryShortName,
      OPPDEPOTREGION_V01.area_cd as OriginAreaCode,
      &apos; o.&apos; || OPPDEPOTREGION_V01.area_cd as OriginAreaLabel,
      (CASE  WHEN OPPDEPOTREGION_V01.bul_id = &apos;&apos; THEN &apos;Unknown&apos;
        WHEN OPPDEPOTREGION_V01.bul_id IS NULL THEN &apos;Unknown&apos;
        ELSE OPPDEPOTREGION_V01.bul_id
        END) as OriginDepotCode,
      &apos; o.&apos;|| (CASE WHEN OPPDEPOTREGION_V01.bul_id = &apos;&apos; THEN &apos;Unknown&apos;
                 WHEN OPPDEPOTREGION_V01.bul_id IS NULL THEN &apos;Unknown&apos;
                 ELSE TRIM(OPPDEPOTREGION_V01.bul_id)
         END)                                       as OriginDepotLabel,
      &apos; o.&apos;|| (CASE WHEN BUSLOC_V01.bul_nm = &apos;&apos; THEN &apos;Dest Depot Unknown&apos;
         WHEN BUSLOC_V01.bul_nm IS NULL THEN &apos;Dest Depot Unknown&apos;
         ELSE BUSLOC_V01.bul_nm
         END)                                       as OriginDepotLongName
FROM      
    /shared/Source/EDW/Ops/PRODOPS_P/OPPDEPOTREGION_V01 OPPDEPOTREGION_V01
INNER JOIN /shared/Source/EDW/Ops/PRODOPS_P/BUSLOC_V01 BUSLOC_V01
ON OPPDEPOTREGION_V01.bul_id = BUSLOC_V01.bul_id
   AND OPPDEPOTREGION_V01.com_id = BUSLOC_V01.com_id
INNER JOIN /shared/Source/EDW/Ops/PRODOPS_P/COUNTRYNAME_V01 COUNTRYNAME_V01
ON OPPDEPOTREGION_V01.cou_id = COUNTRYNAME_V01.cou_id
        AND OPPDEPOTREGION_V01.com_id = COUNTRYNAME_V01.com_id
        AND COUNTRYNAME_V01.cou_non_use_in in (&apos;&apos;, &apos;N&apos;)
        AND COUNTRYNAME_V01.lng_id = &apos;EN&apos;
WHERE  OPPDEPOTREGION_V01.com_id = &apos;WW&apos;
    AND OPPDEPOTREGION_V01.COU_ID &lt;&gt; &apos;ZX&apos;
    AND OPPDEPOTREGION_V01.business_unit_cd &lt;&gt; &apos;OTHER&apos;;</attribute>
