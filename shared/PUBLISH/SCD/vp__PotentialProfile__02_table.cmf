  <attribute name="SQL" type="STRING">SELECT
	T7.&quot;CUS_ID&quot; as Customer_Identifier,
	T7.&quot;TRP_SEQ_NR&quot; as Profile_Sequence_Number,
	T7.&quot;TPC_DELETE_IN&quot; as Profile_Delete_Indicator,
	T6.CMT_NM AS Competitor,
	T7.TPC_REVENUE_AM AS Revenue_Local_Currency,
	(case  when T7.&quot;TPC_CUY_ID&quot; = &apos;EUR&apos; then T7.&quot;TPC_REVENUE_AM&quot; 
	else CAST(T7.&quot;TPC_REVENUE_AM&quot; AS FLOAT) / T8.&quot;EXR_RATE_AM&quot; end ) AS Revenue_Euro, 
	(case  when T7.&quot;TPC_FREQ_CD&quot; = &apos;1&apos; then T7.&quot;TPC_REVENUE_AM&quot; * 5 
	when T7.&quot;TPC_FREQ_CD&quot; = &apos;2&apos; then T7.&quot;TPC_REVENUE_AM&quot; 
	when T7.&quot;TPC_FREQ_CD&quot; = &apos;3&apos; then CAST((T7.&quot;TPC_REVENUE_AM&quot; * 12) AS FLOAT) / 52 
	when T7.&quot;TPC_FREQ_CD&quot; = &apos;4&apos; then CAST((T7.&quot;TPC_REVENUE_AM&quot; * 4) AS FLOAT) / 52 
	else CAST(T7.&quot;TPC_REVENUE_AM&quot; AS FLOAT) / 52 end ) AS Weekly_Revenue_Local_Currency,
	CAST((case  when (T7.&quot;TPC_FREQ_CD&quot; = &apos;1&apos;) then (T7.&quot;TPC_REVENUE_AM&quot; * 5) 
	when (T7.&quot;TPC_FREQ_CD&quot; = &apos;2&apos;) then T7.&quot;TPC_REVENUE_AM&quot; 
	when (T7.&quot;TPC_FREQ_CD&quot; = &apos;3&apos;) then (CAST((T7.&quot;TPC_REVENUE_AM&quot; * 12) AS FLOAT) / 52) 
	when (T7.&quot;TPC_FREQ_CD&quot; = &apos;4&apos;) then (CAST((T7.&quot;TPC_REVENUE_AM&quot; * 4) AS FLOAT) / 52) 
	else (CAST(T7.&quot;TPC_REVENUE_AM&quot; AS FLOAT) / 52) end ) AS FLOAT) / T8.&quot;EXR_RATE_AM&quot; AS Weekly_Revenue_Euro,
	T7.TPC_CUY_ID AS Currency,
	T7.TPC_VALUE_QT AS Quantity,
	T9.TAV_DS AS Quantity_Units,
	T10.TAV_DS AS Frequency
FROM /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMERPROFILE_COMPETTR_V01 T7 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CS_COMPETITOR_V01 T6 
on T7.&quot;COU_ISO_ID_CMT&quot; = T6.&quot;COU_ISO_ID&quot; and T7.&quot;CMT_ID&quot; = T6.&quot;CMT_ID&quot; and &apos;Y&apos; &lt;&gt; T6.&quot;CMT_DELETE_IN&quot; 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CC_EXCHANGERATE_V02 T8 
on T7.&quot;TPC_CUY_ID&quot; = T8.&quot;CUY_ID&quot; and &apos;MO&apos; = T8.&quot;EGP_ID&quot; 
and CURRENT_DATE between T8.&quot;EXR_START_DT&quot; and T8.&quot;EXR_END_DT&quot; 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_TRP_VALUE_UNITS_CD_V01 T9 
on T7.&quot;TPC_VALUE_UNITS_CD&quot; = T9.&quot;TAV_VALUE_ID&quot; and &apos;EN&apos; = T9.&quot;LNG_ID&quot; 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_TRP_FREQ_V01 T10 
on T7.&quot;TPC_FREQ_CD&quot; = T10.&quot;TAV_VALUE_ID&quot; and &apos;EN&apos; = T10.&quot;LNG_ID&quot;</attribute>
