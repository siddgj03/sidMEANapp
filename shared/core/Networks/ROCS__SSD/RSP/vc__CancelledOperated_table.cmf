  <attribute name="SQL" type="STRING">SELECT
	CAST(CANCEL_INDICATOR AS CHAR(1))            AS Cancel_Indicator,
	CAST(CANCEL_INDICATOR_SHORT_DS AS CHAR(20))   AS Cancel_Indicator_Short_Ds,
	CAST(CANCEL_INDICATOR_LONG_DS AS CHAR(20))    AS Cancel_Indicator_Long_Ds,
	CAST(CANCEL_REASON_CD AS CHAR(5))           AS Cancel_Reason_Cd,
	CAST(CANCEL_REASON_SHORT_DS  AS CHAR(5))     AS Cancel_Reason_Short_Ds,
	CAST(CANCEL_REASON_LONG_DS AS CHAR(71))      AS Cancel_Reason_Long_Ds

 FROM
 
 (SELECT
   &apos;Y&apos;                                     AS CANCEL_INDICATOR,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_SHORT_DS,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_LONG_DS,
   RN.RSN_REASON_CD                        AS CANCEL_REASON_CD,
   RN.RSN_REASON_CD                        AS CANCEL_REASON_SHORT_DS,
   CONCAT(RN.RSN_RL_DS,&apos; (Movement)&apos;)                        AS CANCEL_REASON_LONG_DS

   FROM  /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01 RN
   WHERE RN.LNG_ID = &apos;EN&apos;
   AND RN.RSN_REASON_CD LIKE &apos;M%&apos;
   
   UNION

   SELECT
   &apos;Y&apos;                                     AS CANCEL_INDICATOR,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_SHORT_DS,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_LONG_DS,
   RN.RSN_REASON_CD                        AS CANCEL_REASON_CD,
   RN.RSN_REASON_CD                        AS CANCEL_REASON_SHORT_DS,
   CONCAT(RN.RSN_RL_DS,&apos; (Tour)&apos;)                         AS CANCEL_REASON_LONG_DS

   FROM /shared/Source/EDW/ROCS/PRODROCS_P/NETWORKREASON_V01 RN
   WHERE RN.LNG_ID = &apos;EN&apos;
   AND RN.RSN_REASON_CD LIKE &apos;T%&apos;
   
   UNION
 
   SELECT
   &apos;Y&apos;                                     AS CANCEL_INDICATOR,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_SHORT_DS,
   CAST(&apos;Cancelled&apos; AS CHAR(20))           AS CANCEL_INDICATOR_LONG_DS,
   RN.TAV_VALUE_ID                         AS CANCEL_REASON_CD,
   RN.TAV_VALUE_ID                         AS CANCEL_REASON_SHORT_DS,
   RN.TAV_DS                               AS CANCEL_REASON_LONG_DS

   FROM /shared/Source/EDW/ROCS/PRODROCS_P/TOURCANCELLATION_V01  RN
   
   UNION

   SELECT
   &apos;N&apos;                    AS CANCEL_INDICATOR,
   &apos;Scheduled Opererated&apos; AS CANCEL_INDICATOR_SHORT_DS,
   &apos;Scheduled Opererated&apos; AS CANCEL_INDICATOR_LONG_DS,
   &apos;&apos;                     AS CANCEL_REASON_CD,
   &apos;&apos;                     AS CANCEL_REASON_SHORT_DS,
   &apos;&apos;                     AS CANCEL_REASON_LONG_DS

   FROM /shared/Source/EDW/ROCS/PRODROCS_P/TOURCANCELLATION_V01  RN

   ) AS C_O</attribute>
