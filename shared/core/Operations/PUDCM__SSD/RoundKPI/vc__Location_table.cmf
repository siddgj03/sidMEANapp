  <attribute name="SQL" type="STRING">select 
	COU_GRP_ID
	,COU_GRP_NM
	,COU_GRP_SHORT_DS
	,COU_GRP_LONG_DS
	,COU_ISO_ID
	,COU_NM
	,COU_SHORT_DS
	,COU_LONG_DS
	,cast(BUL_CSYS_ID as varchar(40)) as BUL_CSYS_ID
	,BUL_NM
	,BUL_SHORT_DS
	,BUL_LONG_DS
	,REGION_CD
	,REGION_DS
	,REGION_LONG_DS
	,AREA_CD
	,AREA_LONG_DS
	,REP_GRP_CD
	,REP_GRP_DS
	,REP_GRP_LONG_DS
	,COU_LOC
       from
(SELECT
	COU_GRP_ID	AS Cou_Grp_Id,
	COU_GRP_NM	AS Cou_Grp_Nm,
	COU_GRP_SHORT_DS AS Cou_Grp_Short_Ds,
	COU_GRP_LONG_DS	 AS Cou_Grp_Long_Ds,
	COU_ISO_ID	AS Cou_Iso_Id,
	COU_NM	 AS Cou_Nm,
	COU_SHORT_DS	AS Cou_Short_Ds,
	COU_LONG_DS 	AS Cou_Long_Ds,
	cast(BUL_CSYS_ID as varchar(40)) AS Bul_Csys_Id	,
	BUL_NM	 	AS Bul_Nm,
	BUL_SHORT_DS		AS Bul_Short_Ds,
	BUL_LONG_DS	 	AS Bul_Long_Ds,
        REGION_CD       AS Region_Cd,
        REGION_DS     AS Region_Ds,
        REGION_LONG_DS     AS Region_Long_Ds,
        AREA_CD     AS Area_Cd,
        AREA_LONG_DS  AS Area_Long_Ds,
        REP_GRP_CD   AS Rep_Grp_Cd,
        CASE WHEN SUPPRES_CD = &apos;Y&apos; THEN &apos;&apos; ELSE  cast(REGION_CD as varchar(10))        END    AS Rep_Grp_Ds,
        CASE WHEN SUPPRES_CD = &apos;Y&apos; THEN &apos;&apos; ELSE cast(REP_GRP_LONG_DS   as varchar(10))  END   AS Rep_Grp_Long_Ds,

		COU_ISO_ID || BUL_CSYS_ID as COU_LOC

 FROM /shared/Source/EDW/PUDCM/PRODPUDCM_P/LOCATION_V01

 
 UNION
 
 SELECT DISTINCT
	COU_GRP_ID	AS Cou_Grp_Id,
	COU_GRP_NM		AS Cou_Grp_Nm,
	COU_GRP_SHORT_DS	AS Cou_Grp_Short_Ds,
	COU_GRP_LONG_DS	 	AS Cou_Grp_Long_Ds,
	COU_ISO_ID		AS Cou_Iso_Id,
	COU_NM	 		AS Cou_Nm,
	COU_SHORT_DS		AS Cou_Short_Ds,
	COU_LONG_DS	 	AS Cou_Long_Ds,
	CAST(&apos; &apos; AS VARCHAR(40))	AS Bul_Csys_Id,
	&apos; &apos;	 		AS Bul_Nm,
	&apos; &apos;	 AS Bul_Short_Ds,
	&apos; &apos;	  AS Bul_Long_Ds,
        REGION_CD  AS Region_Cd,
        REGION_DS     AS Region_Ds,
        REGION_LONG_DS  AS Region_Long_Ds,
        &apos; &apos;       AS Area_Cd,
        &apos; &apos;       AS Area_Long_Ds,
        REP_GRP_CD    AS Rep_Grp_Cd,
       CASE WHEN SUPPRES_CD = &apos;Y&apos; THEN &apos;&apos; ELSE  cast(REGION_CD as varchar(10))       END      AS Rep_Grp_Ds,
        CASE WHEN SUPPRES_CD = &apos;Y&apos; THEN &apos;&apos; ELSE cast(REP_GRP_LONG_DS as varchar(10))  END      AS Rep_Grp_Long_Ds,

		COU_ISO_ID || BUL_CSYS_ID as COU_LOC
 FROM /shared/Source/EDW/PUDCM/PRODPUDCM_P/LOCATION_V01) a</attribute>
