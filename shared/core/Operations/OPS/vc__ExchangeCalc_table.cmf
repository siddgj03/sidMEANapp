  <attribute name="SQL" type="STRING">SELECT 
      --from /shared/Source/EDW/OPS/PRODOPS_P/EXCHANGERATE_V02
-- [ if  (  Exchange Rate Value Amount  =  0  or  Exchange Rate Value Amount  is missing  ) 
-- then  (  0  )  else  ( cast_decimal (  (  Value of Goods Amount  /  Exchange Rate Value Amount  ) , 18 , 2  )  ) ]
case when EXR_RATE_AM = 0 or EXR_RATE_AM is null then 0 
else CON_VAL_OF_GDS_AM/EXR_RATE_AM    end as &quot;Value_of_Goods_Amount_EUR&quot;,
CON_SRKY_ID &quot;Unique_Consignment_Key&quot;
from /shared/Source/EDW/OPS/PRODOPS_P/CON_V01 t1 
left outer join /shared/Source/EDW/OPS/PRODOPS_P/EXCHANGERATE_V02 t2
on 	 t1.&quot;CUY_ID_VAL_OF_GDS&quot;  =  t2.&quot;CUY_ID&quot; 
and current_date  between  t2.&quot;EXP_START_DT&quot; and  t2.&quot;EXP_END_DT&quot; 
and  t2.&quot;EGP_ID&quot;  =  &apos;MO&apos;     </attribute>
