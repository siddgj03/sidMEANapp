  <attribute name="SQL" type="STRING">SELECT
	CAST(MP_INDICATOR AS CHAR(1))         AS Mp_Indicator,
	CAST(MP_INDICATOR_SHORT_DS AS CHAR(1)) AS Mp_Indicator_Short_Ds,
	CAST(MP_INDICATOR_LONG_DS AS CHAR(9))  AS Mp_Indicator_Long_Ds,
