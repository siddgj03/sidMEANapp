  <attribute name="SQL" type="STRING">SELECT distinct
    CONCAT(CASE WHEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID IS NOT NULL THEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID ELSE &apos;&apos; END, CAST(BUSINESS_PARTNER_V01.BUP_OBJ_ID AS CHAR(18))) OPR_ID, 
    BUSINESS_PARTNER_V01.BUP_ID OPR_SHORT_DS, 
    BUSINESS_PARTNER_V01.BUP_NM OPR_LONG_DS, 
    BUSPARTNER_ADDRESS_V01.COU_ISO_ID COU_ISO_ID, 
    BUSPARTNER_ADDRESS_V01.COU_ISO_ID COU_ISO_ID_SHORT_DS, 
    COUNTRY_V01.COU_NM COU_ISO_ID_LONG_DS, 
    BUSINESS_PARTNER_V01.BUP_OWN_COMPANY_IN BUP_OWN_COMPANY_IN, 
    CASE WHEN BUSINESS_PARTNER_V01.BUP_OWN_COMPANY_IN = &apos;Y&apos;
            THEN &apos;Own&apos;
        ELSE &apos;Subcontractor&apos;
        END BUP_OWN_COMPANY_SHORT_DS, 
    BUSINESS_PARTNER_V01.BUP_ID OPR_ID_SHORT, 
	CONCAT(CONCAT(BUSINESS_PARTNER_V01.BUP_ID,&apos; - &apos;),BUSINESS_PARTNER_V01.BUP_NM) OPR_ID_LONG,
    CONCAT(CASE WHEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID IS NOT NULL THEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID ELSE &apos;&apos; END, CAST(BUSINESS_PARTNER_V01.BUP_OBJ_ID AS CHAR(18))) AS OPR_ID_A, 
    CONCAT(CASE WHEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID IS NOT NULL THEN BUSPARTNER_ADDRESS_V01.COU_ISO_ID ELSE &apos;&apos; END, CAST(BUSINESS_PARTNER_V01.BUP_OBJ_ID AS CHAR(18))) AS OPR_ID_S
FROM      
    /shared/Source/EDW/ROCS/PRODROCS_P/BUSINESS_PARTNER_V01 BUSINESS_PARTNER_V01 LEFT OUTER JOIN 
        /shared/Source/EDW/ROCS/PRODROCS_P/BUSPARTNER_ADDRESS_V01 BUSPARTNER_ADDRESS_V01
    ON BUSINESS_PARTNER_V01.BUP_OBJ_ID = BUSPARTNER_ADDRESS_V01.BUP_OBJ_ID LEFT OUTER JOIN 
        /shared/Source/EDW/ROCS/PRODROCS_P/COUNTRY_V01 COUNTRY_V01
    ON BUSPARTNER_ADDRESS_V01.COU_ISO_ID = COUNTRY_V01.COU_ISO_ID</attribute>
