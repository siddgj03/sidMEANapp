  <attribute name="SQL" type="STRING">SELECT  DISTINCT
T2.&quot;CUS_ID&quot; AS Customer_Id,
T2.&quot;CLO_ID&quot; AS Customer_Location_Id, 
CUF_CD as Location_Type,
(case  when T2.&quot;CLO_ADDR_LINE_1_TX&quot; is not null 
		then T2.&quot;CLO_ADDR_LINE_1_TX&quot; else &apos;&apos; end ) 
	|| (case  when  T2.&quot;CLO_ADDR_LINE_2_TX&quot; is NOT null 
			then &apos; &apos; || T2.&quot;CLO_ADDR_LINE_2_TX&quot; else &apos;&apos; end )
	|| (case  when  NOT T2.&quot;CLO_ADDR_LINE_3_TX&quot; is null 
			then &apos; &apos; || T2.&quot;CLO_ADDR_LINE_3_TX&quot; else &apos;&apos; end ) as Full_Address, 
CLO_POSTAL_CD as Postcode,
CLO_TOWN_NM as Town,
PRV_NM as Province_Or_State_Name,
T2.COU_ISO_ID as Country,
CLO_PBOX_NR as PO_Box_Number    
from /shared/Source/EDW/SCD/PRODSCD_P/CUSTLOCTN_V01 T2 LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/CUSTLOCTNFUNCTN_V01 T1
    on T2.&quot;CUS_ID&quot; = T1.&quot;CUS_ID&quot; and T2.&quot;CLO_ID&quot; = T1.&quot;CLO_ID&quot; and &apos;Y&apos; &lt;&gt; T1.&quot;CLF_DELETE_IN&quot; LEFT OUTER JOIN 
        /shared/Source/EDW/SCD/PRODSCD_P/CS_PROVINCE_V01 T3
    on T2.&quot;COU_ISO_ID_CUS&quot; = T3.&quot;COU_ISO_ID&quot; and T2.&quot;PRV_ID&quot; = T3.&quot;PRV_ID&quot; 
and CURRENT_DATE between T3.&quot;PRV_EFFECT_DT&quot; and T3.&quot;PRV_EFFECT_TO_DT&quot; and &apos;Y&apos; = T3.&quot;PRV_ACTIVE_IN&quot;</attribute>
