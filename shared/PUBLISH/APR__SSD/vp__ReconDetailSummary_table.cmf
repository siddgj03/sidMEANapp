    ISNULL(vc_AllConsignmentStatuses.Consignment_Status_Description,&apos;Unknown&apos;) Consignment_Status_Description, 
    --vc_AllCustomerAccounts.Account_Country_Description Account_Country, 
    --vc_AllCustomerAccounts.Account_Grouping_Description Account_Grouping_Description, 
    --vc_AllCustomerAccounts.Account_Description Account_Description,  
    vc_AllDestinationCountries.Destination_Geozone Destination_Geozone, 
    vc_AllDestinationCountries.Destination_Country_Description Destination_Country_Description, 
    vc_AllDivisionalProducts.Division_Description Division, 
    vc_AllDivisionalProducts.Product_Description Product, 
    vc_AllTransactional.Division_Key Division_Key, 
    ISNULL(vc_AllInvoiceCountries.Region_Name,&apos;Unknown&apos;) Region, 
    --ISNULL(vc_AllInvoiceCountries.Operating_Unit_Name,&apos;Unknown&apos;)Operating_Unit, 
    --vc_AllInvoiceCountries.Reporting_Unit_Name Reporting_Unit, 
    ISNULL(vc_AllInvoiceCountries.Invoice_Country_Description,&apos;Unknown&apos;)Invoice_Country, 
    vc_AllModesOfTransport.MOT_Description Mode_Of_Transport, 
    vc_AllOriginCountries.Origin_Geozone Origin_Geozone, 
    vc_AllOriginCountries.Origin_Country_Description Origin_Country, 
    vc_AllReconciliationStatuses.Reconciliation_Status_Description Reconciliation_Status, 
    vc_AllReconciliationStatuses.Exclusion_Reason_Description Exclusion_Reason , 
    vc_AllSourceSystems.Source_Type Source_Type, 
    vc_AllSourceSystems.Source_System_Description Source_System, 
    vc_TermsOfPayment.TOP_Description Terms_Of_Payment, 
    vc_AllTransactional.Invoice_Country_Code , 
    vc_AllTransactional.No_Of_Consignments, 
    --vc_AllTransactional.TT_Stat_Weight, 
    vc_AllInvoiceCountries.Region_Code, 
    --vc_AllInvoiceCountries.Operating_Unit_Code, 
    --vc_AllInvoiceCountries.Reporting_Unit_Code, 
    vc_AllConCreationDates.Con_Week, 
    vc_AllConCollectionDates.Coll_Week ,
	vc_AllTransactional.TOP_Change_plus_Flag,
	vc_AllTransactional.TOP_Change_Minus_Flag
    --vc_AllTransactional.Consignment_Creation_Date ,
	--vc_AllTransactional.Consignment_Collection_Date 
FROM      
    /shared/Core/Finance/APR_SSD/vc_AllTransactionSummary vc_AllTransactional LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllBDCExclusions vc_AllBDCExclusions
    ON Trim(vc_AllTransactional.BDC_Exclusion_Code) = trim(vc_AllBDCExclusions.BDC_Exclusion_Code) LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllCancellationReasons vc_AllCancellationReasons
    ON vc_AllTransactional.Cancellation_Reason_Code = vc_AllCancellationReasons.Cancellation_Reason_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllConCollectionDates vc_AllConCollectionDates
    ON vc_AllTransactional.Coll_Week = vc_AllConCollectionDates.Coll_Week LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllConCreationDates vc_AllConCreationDates
    ON vc_AllTransactional.Con_Week = vc_AllConCreationDates.Con_Week LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllConsignmentStatuses vc_AllConsignmentStatuses
    ON vc_AllTransactional.Consignment_Status_Code = vc_AllConsignmentStatuses.Consignment_Status_Code LEFT OUTER JOIN 
        
        /shared/Core/Finance/APR_SSD/vc_AllDestinationCountries vc_AllDestinationCountries
    ON vc_AllTransactional.Destination_Country_Code = vc_AllDestinationCountries.Destination_Country_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllDivisionalProducts vc_AllDivisionalProducts
    ON vc_AllTransactional.Division_Key = vc_AllDivisionalProducts.Division_Key LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllErrorDescriptions vc_AllErrorDescriptions
    ON vc_AllTransactional.Interface_Error = vc_AllErrorDescriptions.Interface_Error_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllInvoiceCountriesSummary vc_AllInvoiceCountries
    ON vc_AllTransactional.Invoice_Country_Code = vc_AllInvoiceCountries.Invoice_Country_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllModesOfTransport vc_AllModesOfTransport
    ON vc_AllTransactional.MOT_Code = vc_AllModesOfTransport.MOT_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllOriginCountries vc_AllOriginCountries
    ON vc_AllTransactional.Origin_Country_Code = vc_AllOriginCountries.Origin_Country_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllReconciliationStatusesSummary vc_AllReconciliationStatuses
    ON vc_AllTransactional.Exclusion_Reason_Code = vc_AllReconciliationStatuses.Exclusion_Reason_Code AND 
        vc_AllTransactional.Reconciliation_Status = vc_AllReconciliationStatuses.Reconciliation_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_AllSourceSystems vc_AllSourceSystems
    ON vc_AllTransactional.Source_System_Code = vc_AllSourceSystems.Source_System_Code LEFT OUTER JOIN 
        /shared/Core/Finance/APR_SSD/vc_TermsOfPayment vc_TermsOfPayment
    ON vc_AllTransactional.TOP_Code = vc_TermsOfPayment.TOP_Code	 
	 

