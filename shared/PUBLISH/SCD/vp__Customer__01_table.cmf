  <attribute name="SQL" type="STRING">SELECT 
T1.COU_ISO_ID as Country_Code,
T2.COU_NM as Country_Name,
T1.CUS_ID as Customer_Number,
T1.CUS_DERIVED_CUSTOMER_NM as Customer_Name,
T1.DRK_ID_DELETE as Customer_Delete_Indicator,
T1.DRK_ID_REASON as Customer_Delete_Reason,
T1.CUS_ID_NMP as Customer_NMP,
T1.CUS_ID_IMP as Customer_IMP,
T1.LNG_ID as Language_Id,
T1.CUS_ORGN_TYPE_CD as Customer_Organisational_Code,
--T1.LNG_ID as &quot;Language&quot;, NOT AVAILABLE IN TEST DB
T14.CUS_LCYCL_STAT_CD as Account_Trade_Status_Code,
T1.CUS_DEBTOR_STAT_CD as Customer_Debtor_Code,
T1.CUS_CREATE_DT as Customer_Create_Date,                                  
T1.CUS_VAT_EXEMPT_IN as VAT_Excempt_Indicator,
T15.BIC_ID as SIC_Code,
T15.CPQ_VERT_MRKT_CD as Sector,
T16.TAV_DS as Sector_Description,
T15.CPQ_SUB_SECTOR_CD as Sub_Sector,
T17.TAV_DS as Sub_Sector_Description,
T14.CUS_CLASS_CD as Customer_Classification_Code,
T18.TAV_DS as Customer_Classification_Description,
T14.CUS_CLASS_CD_POT as Customer_Potential_Classification_Code,
T19.TAV_DS as Customer_Potential_Classification_Description,
T1.CUS_INTERNAL_IN as Customer_Internal_Indicator,
T1.CUS_GROUP_CD as Customer_Grouping_Code,
T20.TAV_DS as Customer_Grouping_Description,
T21.TAV_DS as Customer_Type,
T1.CUS_ID_PARENT as Customer_Parent,
T1.CUS_SOURCE_CD as Customer_Source_Code,
(case  when T1.&quot;CUS_COMPANY_NM&quot; is null or T1.&quot;CUS_COMPANY_NM&quot; = &apos;&apos; then &apos;Private&apos; 
else &apos;Company&apos; end ) as Company_Private_Indicator,
 (case  when T23.&quot;APP_ID&quot; = &apos;1&apos; then &apos;myTNT&apos; when T23.&quot;APP_ID&quot; = &apos;2&apos; then &apos;ExpressShipper Connect&apos; 
when T23.&quot;APP_ID&quot; = &apos;3&apos; then &apos;ExpressShipper PDF&apos; when T23.&quot;APP_ID&quot; = &apos;4&apos; then &apos;ExpressManager&apos; 
when T23.&quot;APP_ID&quot; = &apos;5&apos; then &apos;ExpressConnect&apos; when T23.&quot;APP_ID&quot; = &apos;6&apos; then &apos;Autoship&apos; 
when T23.&quot;APP_ID&quot; = &apos;7&apos; then &apos;Boek uw zending (Benelux Only)&apos; 
when T23.&quot;APP_ID&quot; = &apos;8&apos; then &apos;Bespoke Application&apos; 
when T23.&quot;APP_ID&quot; = &apos;9&apos; then &apos;Speedbooking&apos; else NULL end )  as Company_Details_CIT_Solution,
T24.TAV_DS AS Company_Details_No_Of_Employees,
T15.CPQ_TURNOVER_AM as Company_Details_company_Turnover,
T15.CUY_ID_TURNOVER as Company_Details_company_Turnover_Currency 
FROM /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_V02 T1
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CS_COUNTRY_V01 T2 
 on T1.&quot;COU_ISO_ID&quot; = T2.&quot;COU_ISO_ID&quot;
  LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_NONCORE_V01 T14
 on T1.&quot;CUS_ID&quot; = T14.&quot;CUS_ID&quot;
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CUS_CLASS_POT_TYPE_V01 T19 
 on T14.&quot;CUS_CLASS_CD_POT&quot; = T19.&quot;CUS_CLASS_CD_POT&quot; and &apos;EN&apos; = T19.&quot;LNG_ID&quot;
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CUS_CLASS_TYPE_V01 T18 
 on T14.&quot;CUS_CLASS_CD&quot; = T18.&quot;CUS_CLASS_CD&quot; and T18.&quot;LNG_ID&quot; = &apos;EN&apos;
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CUS_GROUP_TYPE_V01 T20 
 on T1.&quot;CUS_GROUP_CD&quot; = T20.&quot;CUS_GROUP_CD&quot; and &apos;EN&apos; = T20.&quot;LNG_ID&quot; 
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CUSTAPPLCTN_V01 T23 
 on T1.&quot;CUS_ID&quot; = T23.&quot;CUS_ID&quot; and &apos;Y&apos; &lt;&gt; T23.&quot;CAZ_DELETE_IN&quot;
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CUSTOMER_BIC_V01 T15 
 on T1.&quot;CUS_ID&quot; = T15.&quot;CUS_ID&quot; and &apos;Y&apos; &lt;&gt; T15.&quot;CPQ_DELETE_IN&quot; 
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CPQ_COMP_SIZE_TYPE_V01 T24 
 on T15.&quot;CPQ_COMP_SIZE&quot; = T24.&quot;CPQ_COMP_SIZE&quot; and &apos;EN&apos; = T24.&quot;LNG_ID&quot;
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CUS_ORGN_TYPE_V01 T21 
 on T1.&quot;CUS_ORGN_TYPE_CD&quot; = T21.&quot;CUS_ORGN_TYPE_CD&quot; and &apos;EN&apos; = T21.&quot;LNG_ID&quot; 
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CPQ_VERT_MRKT_V01 T16 
 on T15.&quot;CPQ_VERT_MRKT_CD&quot; = T16.&quot;CPQ_VERT_MRKT_CD&quot; and &apos;EN&apos; = T16.&quot;LNG_ID&quot; 
 LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_CPQ_SUB_SECTOR_V01 T17 
 on T15.&quot;CPQ_SUB_SECTOR_CD&quot; = T17.&quot;CPQ_SUB_SECTOR_CD&quot; and &apos;EN&apos; = T17.&quot;LNG_ID&quot;</attribute>
