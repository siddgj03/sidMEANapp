  <attribute name="SQL" type="STRING">SELECT 
T3.&quot;OPP_ID&quot; as Unique_Opportunity_Number,
T3.&quot;OPH_DELETE_IN&quot; as Opportunity_Delete_Indicator,
TAV_DS as Status,
OPH_EVENT_TD as Status_DateTime,
OPH_EVENT_DT as status_Date,
case  when (TRIM(T4.&quot;EMP_FIRST_NM&quot;) || TRIM(T4.&quot;EMP_SUR_NM&quot;) is null) 
	then T3.&quot;OPH_UPDT_USER_ID&quot; 
	else TRIM(T4.&quot;EMP_FIRST_NM&quot;) || &apos; &apos; || TRIM(T4.&quot;EMP_SUR_NM&quot;) end as Status_User, 
TRIM(T4.&quot;EMP_FIRST_NM&quot;)|| &apos; &apos; || TRIM(T4.&quot;EMP_SUR_NM&quot;) as MA_Manager 
FROM /shared/Source/EDW/SCD/PRODSCD_P/SALES_OPPORTUNITY_HIST_V01 T3
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/AV_OPP_STATUS_V01 T2 
on T3.&quot;OPP_STATUS_CD&quot; = T2.&quot;TAV_VALUE_ID&quot; and &apos;EN&apos; = T2.&quot;LNG_ID&quot; 
LEFT OUTER JOIN /shared/Source/EDW/SCD/PRODSCD_P/CS_EMPLOYEE_V01 T4 
on T3.&quot;OPH_UPDT_USER_ID&quot; = T4.&quot;EMP_USER_ID&quot; and &apos;&apos; &lt;&gt; T4.&quot;EMP_USER_ID&quot; and &apos;Y&apos; &lt;&gt; T4.&quot;EMP_DELETE_IN&quot; 
and CURRENT_DATE between T4.&quot;EMP_START_DT&quot; and T4.&quot;EMP_END_DT&quot; </attribute>
